<?xml version="1.0" ?>

<!--
     PersonSchema.xml

     BZIssue::1128 (added UpdateMode element and accompanying custom rules)
     BZIssue::3130 (changed Comment so it has no maximum occurence)
     BZIssue::4843 (GPUsage type added)
     BZIssue::4957 (added genetics societies CGA-ICC and InSiGHT)
     BZIssue::5071 (comment about obsolete board member information block)
     BZIssue::5248 (new GeneticsBoardName value)
  -->

<xsd:schema              xmlns:xsd       = 'http://www.w3.org/2001/XMLSchema'>

<!--=================================================================-->
<!--                        CUSTOM RULE                              -->
<!--=================================================================-->
 <xsd:annotation>
  <xsd:appinfo>
   <pattern>
    <rule context = '/Person/PersonLocations'>
     <assert>
      <test><![CDATA[
not(.//PersonRole
                                        [. = "Protocol update person"])
                                        or UpdateMode
]]>
      </test>
      <message><![CDATA[
UpdateMode attribute required when PUP role is present
]]>
      </message>
     </assert>
    </rule>
    <rule context = '/Person/PersonLocations'>
     <assert>
      <test><![CDATA[
not(CIPSContact)
                or CIPSContact=Home/@cdr-id
                or CIPSContact=PrivatePractice/PrivatePracticeLocation/@cdr-id
                or CIPSContact=OtherPracticeLocation/@cdr-id
]]>
      </test>
      <message><![CDATA[
CIPSContact not found among PersonLocation subelements
]]>
      </message>
     </assert>
    </rule>
   </pattern>
  </xsd:appinfo>
 </xsd:annotation>

<!--=================================================================-->
<!--                COMMON CDR SCHEMA COMPONENTS                     -->
<!--=================================================================-->
  <xsd:include           schemaLocation  = 'CdrCommonSchema.xml'/>

<!--=================================================================-->
<!--                 TOP-LEVEL DOCUMENT ELEMENT                      -->
<!--=================================================================-->
  <xsd:element           name            = 'Person'
                         type            = 'Person'/>

<!--=================================================================-->
<!--                       DOCUMENT TYPE                             -->
<!--=================================================================-->
  <xsd:complexType       name            = 'Person'>
    <xsd:sequence>
      <xsd:element       name            = 'PersonNameInformation'
                         type            = 'PersonNameInformation'/>
      <xsd:element       name            = 'ProfessionalInformation'
                         type            = 'ProfessionalInformation'
                         minOccurs       = '0'/>
      <xsd:element       name            = 'PersonLocations'
                         type            = 'PersonLocations'/>
      <xsd:element       name            = 'ProfessionalType'
                         type            = 'ProfessionalType'
                         minOccurs       = '0'
                         maxOccurs       = 'unbounded'/>
      <xsd:element       name            = 'Status'
                         type            = 'Status'/>
      <xsd:element       name            = 'DatedAction'
                         type            = 'DatedAction'
                         minOccurs       = '0'
                         maxOccurs       = 'unbounded'/>
      <xsd:element       name            = 'DateLastModified'
                         type            = 'xsd:date'
                         minOccurs       = '0'/>
      <xsd:element       name            = 'PdqKey'
                         type            = 'NotEmptyString'
                         minOccurs       = '0'/>
    </xsd:sequence>
  </xsd:complexType>

