<!-- 
     $Id$ 

     BZIssue::1128 (added UpdateMode element and accompanying custom rules)
     BZIssue::3130 (changed Comment so it has no maximum occurence)
     BZIssue::4843 (GPUsage type added)
  -->

<schema              xmlns           = 'http://www.w3.org/2001/XMLSchema'>

<!--=================================================================-->
<!--                        CUSTOM RULE                              -->
<!--=================================================================-->
 <annotation>
  <appInfo>
   <pattern>
    <rule            context         = '/Person/PersonLocations'>
     <assert         test            = 'not(.//PersonRole
                                        [. = "Protocol update person"])
                                        or UpdateMode'
     >UpdateMode attribute required when PUP role is present</assert>
    </rule>
    <rule            context         = '/Person/PersonLocations'>
     <assert         test            = 'not(CIPSContact)
                or CIPSContact=Home/@cdr:id
                or CIPSContact=PrivatePractice/PrivatePracticeLocation/@cdr:id
                or CIPSContact=OtherPracticeLocation/@cdr:id'
     >CIPSContact not found among PersonLocation subelements</assert>
    </rule>
   </pattern>
  </appInfo>
 </annotation>

<!--=================================================================-->
<!--                COMMON CDR SCHEMA COMPONENTS                     -->
<!--=================================================================-->
  <include           schemaLocation  = 'CdrCommonSchema.xml'/>

<!--=================================================================-->
<!--                 TOP-LEVEL DOCUMENT ELEMENT                      -->
<!--=================================================================-->
  <element           name            = 'Person'
                     type            = 'Person'/>

<!--=================================================================-->
<!--                       DOCUMENT TYPE                             -->
<!--=================================================================-->
  <complexType       name            = 'Person'>
    <sequence>
      <element       name            = 'PersonNameInformation'
                     type            = 'PersonNameInformation'/>
      <element       name            = 'ProfessionalInformation'
                     type            = 'ProfessionalInformation'
                     minOccurs       = '0'/>
      <element       name            = 'PersonLocations'
                     type            = 'PersonLocations'/>
      <element       name            = 'ProfessionalType'
                     type            = 'ProfessionalType'
                     minOccurs       = '0'
                     maxOccurs       = 'unbounded'/>
      <element       name            = 'Status'
                     type            = 'Status'/>
      <element       name            = 'DatedAction'
                     type            = 'DatedAction'
                     minOccurs       = '0'
                     maxOccurs       = 'unbounded'/>
