                     type            = 'string'/>
      <element       name            = 'IsSection801'
                     type            = 'string'/>
      <element       name            = 'DelayedPosting'
                     type            = 'string'
                     minOccurs       = '0'/>
      <element       name            = 'CTOversightInfo'
                     type            = 'CTOversightInfo'/>
      <element       name            = 'TrialIndIde'
                     type            = 'TrialIndIde'
                     minOccurs       = '0'/>
      <element       name            = 'TrialFunding'
                     type            = 'TrialFunding'
                     minOccurs       = '0'/>
      <element       name            = 'OverallStatus'
                     type            = 'string'/>
      <element       name            = 'OverallStatusDate'
                     type            = 'date'/>
      <element       name            = 'ReasonStopped'
                     type            = 'string'
                     minOccurs       = '0'/>
      <element       name            = 'StartDate'
                     type            = 'date'
                     minOccurs       = '0'/>
      <element       name            = 'StartDateType'
                     type            = 'string'/>
      <element       name            = 'CompletionDate'
                     type            = 'date'/>
      <element       name            = 'CompletionDateType'
                     type            = 'string'/>
      <element       name            = 'BriefSummary'
                     type            = 'ParasBlock'
                     minOccurs       = '0'/>
      <element       name            = 'DetailedDescription'
                     type            = 'ParasBlock'
                     minOccurs       = '0'/>
      <element       name            = 'CTStudyDesign'
                     type            = 'CTStudyDesign'/>
      <element       name            = 'CTOutcomes'
                     type            = 'CTOutcomes'
                     minOccurs       = '0'/>
      <element       name            = 'CTEnrollment'
                     type            = 'CTEnrollment'
                     minOccurs       = '0'/>
      <element       name            = 'CTEligibility'
                     type            = 'CTEligibility'
                     minOccurs       = '0'/>
      <element       name            = 'ArmsOrGroups'
                     type            = 'ArmsOrGroups'/>
      <element       name            = 'SubGroups'
                     type            = 'SubGroups'
                     minOccurs       = '0'/>
      <element       name            = 'CTRPIndexing'
                     type            = 'CTRPIndexing'/>
      <element       name            = 'Location'
                     type            = 'CTLocation'
                     minOccurs       = '0'
                     maxOccurs       = 'unbounded'/>
      <element       name            = 'ProtocolProcessingDetails'
                     type            = 'ProtocolProcessingDetails'
                     minOccurs       = '0'
                     maxOccurs       = 'unbounded'/>
      <element       name            = 'PDQIndexing'
                     type            = 'PDQIndexing'
                     minOccurs       = '0'/> <!-- XXX FOR TESTING -->
      <element       name            = 'VerificationDate'
                     type            = 'string'/>
    </sequence>
    <attribute       name            = 'xmlns:cdr'
                     type            = 'anyURI'
                     minOccurs       = '0'/>
  </complexType>

