                     type            = 'string'
                     minOccurs       = '0'/>
      <element       name            = 'OfficialTitle'
                     type            = 'string'
                     minOccurs       = '0'/>
      <element       name            = 'LeadOrg'
                     type            = 'CTOrg'/>
      <element       name            = 'Sponsors'
                     type            = 'CTSponsors'/>
      <element       name            = 'TrialOwners'
                     type            = 'TrialOwners'/>
      <element       name            = 'ResponsibleParty'
                     type            = 'ResponsibleParty'/>
