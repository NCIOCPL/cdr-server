      <xsd:element       name            = 'ProcessingStatuses'
                         type            = 'ProcessingStatuses'
                         minOccurs       = '0'/>
      <xsd:element       name            = 'MissingRequiredInformation'
                         type            = 'MissingRequiredInformation'
