<?xml version="1.0"?>
  <xsl:transform xmlns:xsl="http://www.w3.org/1999/XSL/Transform"
                 version="1.0">
   <xsl:output method="text"/>
  <!--Filter written by C.Burg - 8-30-2001 -->
  <!-- This program is a filter to display a pubmed citation
       titles -->
        
<!-- display the PMID data element-->    
<xsl:template match="Citation">
<xsl:for-each select="PubmedArticle/MedlineCitation">
<xsl:value-of select="PMID"/>;<xsl:text/>
</xsl:for-each>
<!-- Display the article data element -->
<xsl:for-each select="PubmedArticle/MedlineCitation/Article">
<xsl:value-of select="ArticleTitle"/>;<xsl:text/>
</xsl:for-each>
<!-- Display the journal -->
<xsl:for-each select="PubmedArticle/MedlineCitation">
<xsl:value-of select="MedlineJournalInfo/MedlineTA"/>
</xsl:for-each>
</xsl:template>
</xsl:transform>