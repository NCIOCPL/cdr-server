<CdrDoc Type='Filter' Id='CDR0000000088'>
<CdrDocCtl>
<DocValStatus readonly="yes">U</DocValStatus>
<DocActiveStatus readonly="yes">A</DocActiveStatus>
<DocTitle readonly="yes">Terminology QC Report Filter</DocTitle>
<ReadyForReview readonly="yes">N</ReadyForReview>
</CdrDocCtl>
<CdrDocXml><![CDATA[<?xml version="1.0"?>

<!-- ===================================================================
      Filter to display terminology QC Report
     ================================================================== -->

  <xsl:transform xmlns:xsl="http://www.w3.org/1999/XSL/Transform"

                 version="1.0"

                 xmlns:cdr="cips.nci.nih.gov/cdr">
   <xsl:import    href = "cdr:name:InLine Markup Formatter"/>
   <xsl:include   href = "cdr:name:Table Formatter"/>

   <xsl:output method="html"/>

   <xsl:template match="/">

   <!--Filter written by C.Burg   10/18/2001 -->
   <!-- Made Tabular - 6/26/2003 - Nanci -->

   <!-- <!DOCTYPE HTML PUBLIC '-//IETF//DTD HTML//EN' /> -->

    <HTML>

    <HEAD>

     <xsl:variable name="cdrmydoc" select="document('cdrx:/*/CdrCtl')"/>

       <TITLE><xsl:value-of select="document('cdrutil:/docid')"/>               &#xa0;&#xa0;<xsl:value-of select="substring-before($cdrmydoc/CdrDocCtl/DocTitle,';')"/> 

 </TITLE>
 <STYLE type='text/css'>
      ol.a { list-style: lower-alpha; }
       ol.A { list-style: upper-alpha; }
       ol.i { list-style: lower-roman; }
       ol.I { list-style: upper-roman; }
       ol.d { list-style: decimal; }
 
 </STYLE>
     

     <!--

     <LINK REL='stylesheet' 

           HREF='/stylesheets/protocol2.css' 

           TYPE='text/css' /> -->

    </HEAD>

   <BASEFONT FACE='Arial, Helvetica, sans-serif' />

    <xsl:apply-templates select="Term"/>

    

    </HTML>

    </xsl:template>

    <xsl:template match="Term">

    <body>

   <!-- ================================================================

                  Display Report heading and subheading
        =============================================================== -->

  <b><font size='4'><center>Terminology<br />QC Report

     </center></font></b>

 <b><font size='3'><center><xsl:value-of select="document('cdrutil:/date/%25B %25#d, %25Y %25#I:%25M%25p')"/></center></font></b><br /><br />

 <b><font size='4'><xsl:apply-templates select="DocId"/></font></b><br />

 <!-- ===================================================================
                Display preferred name of term 
      ================================================================= -->

  <table width = '90%'>
    <tr>
      <td width = '35%'><b><font size='3'>Preferred Name</font></b></td>
      <td width = '5%'>&#xa0;</td>
      <td width = '60%'><xsl:apply-templates select="PreferredName"/></td>
    </tr>
  </table>
  <br />

<!-- ====================================================================
        If other names, display them, including type,review status 
     =================================================================== -->

  <b><font size='3'>Other Names</font></b>

  <xsl:for-each select="OtherName">
    <table width = '90%'>
      <tr>
        <td width = '35%'>
          &#xa0;&#xa0;&#xa0;&#xa0;<b><font size='3'>Term</font></b>
        </td>
        <td width = '5%'>&#xa0;</td>
        <td width = '60%'><xsl:value-of select="OtherTermName"/></td>
      </tr>
      <tr>
        <td>&#xa0;&#xa0;&#xa0;&#xa0;<b><font size='3'>Type</font></b></td>
        <td></td>
        <td><xsl:value-of   select = "OtherNameType"/></td>
      </tr>

      <xsl:if test="not(SourceInformation)">
        <tr>
          <td>
            &#xa0;&#xa0;&#xa0;&#xa0;&#xa0;
            <b><font size='3'>Source Information</font></b>
          </td>
<!--          <td>&#xa0;</td>
          <td>&#xa0;</td>  -->
        </tr>
      </xsl:if>
    </table>

    <xsl:if test="SourceInformation">

      <xsl:text>&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;</xsl:text>
      <b><font size='3'>Source Information</font></b>

      <table width = '90%'>
        <tr>
          <td width = '50%'>
            &#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;
            &#xa0;&#xa0;&#xa0;&#xa0;&#xa0;
            <b><font size='3'>Reference Source</font></b>
          </td>
          <td width = '5%'>&#xa0;</td>
          <td width = '45%'>
            <xsl:value-of select = "SourceInformation/ReferenceSource"/>
          </td>
        </tr>
        <tr>
          <td>
            &#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;
            &#xa0;&#xa0;&#xa0;&#xa0;&#xa0;
            <b><font size='3'>Vocabulary Source</font></b>
          </td>
          <td></td>
          <td>
            <xsl:value-of select = "SourceInformation/ReferenceSource
                                    /VocbularySource"/>
          </td>
        </tr>
        <tr>
          <td>
            &#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;
            &#xa0;&#xa0;&#xa0;&#xa0;&#xa0;
            <b><font size='3'>Vocabulary Source Term Type</font></b>
          </td>
          <td></td>
          <td>
            <xsl:value-of select = "SourceInformation/ReferenceSource/
                                    VocbularySource/SourceTermType"/>
          </td>
        </tr>
        <tr>
          <td>
            &#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;
            &#xa0;&#xa0;&#xa0;&#xa0;&#xa0;
            <b><font size='3'>Vocabulary Source Term ID</font></b>
          </td>
          <td></td>
          <td>
            <xsl:value-of select = "SourceInformation/ReferenceSource/
                                    VocbularySource/SourceTermId"/>
          </td>  
        </tr>
     </table>
   </xsl:if>   

    <table width = '90%'>
      <tr>
        <td width = '35%'>
          &#xa0;&#xa0;&#xa0;&#xa0;&#xa0;
          <b><font size='3'>Review Status</font></b></td>
        <td width = '5%'>&#xa0;</td>
        <td width = '60%'><xsl:value-of  select = "ReviewStatus"/></td>
      </tr>
    </table>

    <xsl:text>&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;</xsl:text>
    <b><font size='3'>Comment</font></b>
    <xsl:text>&#xa0;&#xa0;</xsl:text>
    <xsl:value-of select = "Comment"/>

    <br /><br />

  </xsl:for-each>

<!-- ======================================================================
                         Display definitions
     ==================================================================== -->

<!-- </dl> -->

<xsl:if test="not(Definition)">

<b><font size='3'>Definition</font></b><br /><br />

</xsl:if>

 <xsl:if test="Definition">

<b><font size='3'>Definition</font></b><br />

 <xsl:text>&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;</xsl:text><b><font size='3'>Text</font></b><br />
<xsl:text>&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;</xsl:text>
 <xsl:apply-templates select="Definition/DefinitionText"/>
 <br /><br />

 <xsl:text>&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;</xsl:text><b><font size='3'>Type</font></b><br />
 <xsl:text>&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;</xsl:text>
 
<xsl:value-of select="Definition/DefinitionType"/>
 <br /><br />

  <xsl:text>&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;</xsl:text><b><font size='3'>Source(Name,ID)</font></b><br />
  <xsl:text>&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;</xsl:text>
  <xsl:value-of select="Definition/DefinitionSource"/><br /><br />
  

 <xsl:text>&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;</xsl:text><b><font size='3'>Comment</font></b><br />
 <xsl:text>&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;</xsl:text>
 <xsl:value-of select="Definition/Comment"/>
 <br /><br /> 
</xsl:if> 

<!-- ==================================================================
                       Display term type 
 ======================================================================= -->

  <xsl:if test="TermType">
    <b><font size='3'>Term Type</font></b>
    <br />
    <table width = '90%'>
      <tr>
        <td width = '35%'>&#xa0;&#xa0;<b><font size='3'>Name</font></b></td>
        <td width = '5%'>&#xa0;</td>
        <td width = '60%'>
          <table>
            <xsl:for-each  select = "TermType/TermTypeName">
              <tr>
                <td><xsl:value-of select = "."/></td>
              </tr>
            </xsl:for-each>
          </table> 
        </td>
      </tr>
    </table>

  </xsl:if>
  <br />
 
 
<!-- =================================================================
                   Display semantic type 
    =================================================================== -->

  <xsl:if test="SemanticType">

    <table width = '90%'>
      <tr>
        <td width = '35%'><b><font size='3'>Semantic Type</font></b></td>
        <td width = '5%'>&#xa0;</td>
        <td width = '60%'>
          <table>
            <xsl:for-each select = "SemanticType">
              <tr>
                <td><xsl:value-of select="."/></td>
              </tr>
            </xsl:for-each>
          </table>
        </td>
      </tr>
    </table>

  </xsl:if>

<!-- ===================================================================
               Display Parent term,ID, PDQ Key,and type 
     =================================================================== -->

<!--<xsl:if test="TermRelationship/ParentTerm">-->

<br />

<b><font size='3'>Term Relationship</font></b>

<br />

 <xsl:for-each  select="TermRelationship/ParentTerm">
  <table width = '90%'>
    <tr>
      <td width = '35%'>
        &#xa0;&#xa0;&#xa0;&#xa0;<b><font size='3'>Parent Term</font></b>
      </td>
      <td width = '5%'>&#xa0;</td>
      <td width = '60%'><xsl:value-of  select = "PreferredName"/></td>
    </tr>
    <tr>
      <td>&#xa0;&#xa0;&#xa0;&#xa0;<b><font size='3'>Term ID</font></b></td>
      <td></td>
      <td><xsl:value-of select="@cdr:ref"/></td>
    </tr>
    <tr>
      <td>
        &#xa0;&#xa0;&#xa0;&#xa0;<b><font size='3'>Parent PDQ Key</font></b>
      </td>
      <td></td>
      <td><xsl:value-of select="PdqKey"/></td>
    </tr>
    <tr>
      <td>&#xa0;&#xa0;&#xa0;&#xa0;<b><font size='3'>Parent Type</font></b></td>
      <td></td>
      <td><xsl:value-of select="ParentType"/></td>
    </tr>
  </table>

    <xsl:if test="not(Comment)">
      <xsl:text>&#xa0;&#xa0;&#xa0;&#xa0;</xsl:text>
      <b><font size='3'>Comment</font></b>
      <br /><br />
    </xsl:if>

    <xsl:if test="Comment">
      <xsl:text>&#xa0;&#xa0;&#xa0;&#xa0;</xsl:text>
      <b><font size='3'>Comment</font></b>
      <xsl:value-of select="Comment"/>
      <br /><br />
    </xsl:if> 

  </xsl:for-each>

  <br />

  <xsl:if test="not(TermRelationship/RelatedTerm)">
    <xsl:text>&#xa0;&#xa0;&#xa0;</xsl:text>
    <b><font size='3'>Related Term</font></b>
    <br />
  </xsl:if>

  
<!-- ===================================================================
            Display related terms, ID, PDQ Key, and type 
     ==================================================================== -->

  <xsl:if test="TermRelationship/RelatedTerm">

    <xsl:for-each select="TermRelationship/RelatedTerm">

      <table width = '90%'>
        <tr>
          <td width = '35%'>
            &#xa0;&#xa0;&#xa0;<b><font size='3'>Related Term</font></b>
          </td>
          <td width = '5%'>&#xa0;</td>
          <td width = '60%'><xsl:value-of select="PreferredName"/></td>
        </tr>
        <tr>
          <td>
            &#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;
            <b><font size='3'>Term ID</font></b>
          </td>
          <td></td>
          <td><xsl:value-of select="@cdr:ref"/></td>
        </tr>
        <tr>
          <td>
            &#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;
            <b><font size='3'>Related PDQ Key</font></b>
          </td>
          <td></td>
          <td><xsl:value-of select="PdqKey"/></td>
        </tr>
        <xsl:if  test = "RelationshipType">
          <tr>
            <td>
              &#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;
              <b><font size='3'>Relationship Type</font></b>
            </td>
            <td></td>
            <td><xsl:value-of select="RelationshipType"/></td>
          </tr>
        </xsl:if>
      </table>

      <xsl:text>&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;</xsl:text>
      <b><font size='3'>Comment</font></b>
      <xsl:value-of select="Comment"/>
      <br /><br />

    </xsl:for-each>

  </xsl:if>
  <br />

<!-- ===================================================================
                  Display term status and comment if available 
     ==================================================================== -->

  <b><font size='3'>Term Status</font></b><br />

  <table width = '90%'>
    <tr>
      <td width = '35%'>
        &#xa0;&#xa0;&#xa0;&#xa0;<b><font size='3'>Status</font></b>
      </td>
      <td width = '5%'>&#xa0;</td>
      <td width = '60%'>
        <table>
          <xsl:for-each select="TermStatus">
            <tr>
              <td><xsl:value-of select="."/></td>
            </tr>
          </xsl:for-each>
        </table>
      </td>
    </tr>
  </table>
  <xsl:text>&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;</xsl:text>
  <b><font size='3'>Comment</font></b><br />

  <xsl:for-each select="Comment">
    <xsl:text>&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;</xsl:text>
    <xsl:value-of select="."/>
  </xsl:for-each>

  <br /><br />

 <!-- =================================================================
                       Display Menu Information
      ================================================================= -->
          
  <b><font size='3'>Menu Information</font></b>
  <br /><br />
  <xsl:for-each select="MenuInformation/MenuItem"> 
    <xsl:value-of select="position()"/>.
    <b><font size='3'>Menu Item</font></b>
    <br />
    <table width = '90%'>
      <tr>
        <td width = '35%'>
          &#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;
          <b><font size='3'>Menu Type</font></b>
        </td>
        <td width = '5%'>&#xa0;</td>
        <td width = '60%'><xsl:value-of select="MenuType"/></td>
      </tr>
    </table>

    <xsl:for-each select="MenuParent">
      <table width = '90%'>
        <tr>
          <td width = '35%'>
            &#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;
            <b><font size='3'>MenuParent</font></b>
          </td>
          <td width = '5%'>&#xa0;</td>
          <td width = '60%'>&#xa0;</td>
        </tr>
        <tr>
          <td>
            &#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;
            <b><font size='3'>Display Name</font></b>
          </td>
          <td></td>
          <td><xsl:value-of select="DisplayName"/></td>
        </tr>
      </table>
    </xsl:for-each>

    <table width = '90%'>
      <tr>
        <td width = '35%'>&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;
          <b><font size='3'>Sort Order</font></b>
        </td>
        <td width = '5%'>&#xa0;</td>
        <td width = '60%'><xsl:value-of select="@SortOrder"/></td>
      </tr>
      <tr>
        <td>
          &#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;
          <b><font size='3'>MenuStatus</font></b>
        </td>
        <td></td>
        <td><xsl:value-of select="MenuStatus"/></td>
      </tr>
      <tr>
        <td>
          &#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;
          <b><font size='3'>Entered By</font></b>
        </td>
        <td></td>
        <td><xsl:value-of select="EnteredBy"/></td>
      </tr>
      <tr>
        <td>
          &#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;
          <b><font size='3'>EntryDate</font></b>
        </td>
        <td></td>
        <td><xsl:value-of select="EntryDate"/></td>
      </tr>
    </table>

    <xsl:text>&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;</xsl:text>
    <b><font size='3'>Comment</font></b>
    <br />
    <xsl:text>&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;</xsl:text>
    <xsl:value-of select="Comment"/>
    <br /><br  />

  </xsl:for-each>
  
<!-- ===================================================================
                         Display Record Information
      ================================================================ -->

  <b><font size='3'>Record Information</font></b><br /><br />

  <xsl:variable name="cdrmydoc" select="document('cdrx:/*/CdrCtl')"/>

  <table width = '90%'>
    <tr>
      <td width = '25%'><b>Date Created</b></td>
      <td width = '5%'>&#xa0;</td>
      <td width = '70%'>
        <xsl:value-of select="$cdrmydoc/CdrDocCtl/Create/Date"/>
      </td>
    </tr>
    <tr>
      <td><b>User</b></td>
      <td></td>
      <td><xsl:value-of select="$cdrmydoc/CdrDocCtl/Create/User"/></td>
    </tr>
    <tr>
      <td><b>Date Modified</b></td>
      <td></td>
      <td><xsl:value-of select="$cdrmydoc/CdrDocCtl/Modify/Date"/></td>
    </tr>
    <tr>
      <td><b>User</b></td>
      <td></td>
      <td><xsl:value-of select="$cdrmydoc/CdrDocCtl/Modify/User"/></td>
    </tr>
  </table>

 
<!-- Comment below required exactly as given for Concept/Term Review Report--> 

   <xsl:comment>@@TERMTREE@@</xsl:comment>



  </body>

   </xsl:template>
  </xsl:transform>]]>
</CdrDocXml>
</CdrDoc>