<?xml version='1.0' encoding='UTF-8'?>

<!--
      $Id: CDR0000000039.xml,v 1.1.1.1 2002-07-01 15:34:20 bkline Exp $

      Filter to extract CDR citation IDs from summary document.

      $Log: not supported by cvs2svn $

  -->

<xsl:transform xmlns:xsl = "http://www.w3.org/1999/XSL/Transform"
               version   = "1.0"
               xmlns:cdr = "cips.nci.nih.gov/cdr">
 <xsl:output method="text"/>

 <!-- Just process CitationReference elements -->
 <xsl:template match = '/'>
  <xsl:apply-templates select="//CitationReference"/>
 </xsl:template>

 <!-- Expand citation references -->
 <xsl:template match="CitationReference">
  <xsl:if test='string-length(@cdr:ref) > 3'>
   <xsl:value-of select='substring(@cdr:ref, 4)'/><xsl:text>
</xsl:text>
  </xsl:if>
 </xsl:template>

</xsl:transform>
