<CdrDoc Type='Filter' Id='CDR0000000090'>
<CdrDocCtl>
<DocValStatus readonly="yes">U</DocValStatus>
<DocActiveStatus readonly="yes">A</DocActiveStatus>
<DocTitle readonly="yes">Glossary Term QC Report filter</DocTitle>
<ReadyForReview readonly="yes">N</ReadyForReview>
</CdrDocCtl>
<CdrDocXml><![CDATA[<?xml version="1.0"?>
   <xsl:transform xmlns:xsl="http://www.w3.org/1999/XSL/Transform"
                    version="1.0"
                  xmlns:cdr="cips.nci.nih.gov/cdr">
   <xsl:import    href = "cdr:name:InLine Markup Formatter"/>
   <xsl:output method="html"/>

   <!--Filter written by C.Burg 9/25/2001 -->
   <!-- Made Tabular 6/20/2003 - Nanci -->
   <!-- ======================================================
         This program is a filter to create a Glossary term
         Content QC Report 
        ======================================================-->

   <xsl:template match="/">

   <!-- <!DOCTYPE HTML PUBLIC '-//IETF//DTD HTML//EN' /> -->
   <HTML>
      <HEAD>
         <xsl:variable name="cdrmydoc" select="document('cdrx:/*/CdrCtl')"/>
         <TITLE>
            <xsl:value-of select="document('cdrutil:/docid')"/>&#xa0;&#xa0;
            <xsl:value-of select="$cdrmydoc/CdrDocCtl/DocTitle"/> 
         </TITLE>
     
         <!--
         <LINK REL='stylesheet' 
              HREF='/stylesheets/protocol2.css' 
              TYPE='text/css' /> -->
         <STYLE>
         li.lnone {list-style-type: none}
              
       ol.little-alpha { list-style: lower-alpha; }
       ol.A { list-style: upper-alpha; }
       ol.little-roman { list-style: lower-roman; }
       ol.I { list-style: upper-roman; }
       ol.d { list-style: decimal; }
      </STYLE>
      
      </HEAD>
      <BASEFONT FACE='Arial, Helvetica, sans-serif' />
      <xsl:apply-templates select="GlossaryTerm"/>
   </HTML>
   </xsl:template>

  <!-- ===================================================  
        Generate the HTML for the glossary term QC Report
       ===================================================-->
   <xsl:template match="GlossaryTerm">
      <BODY>
      <!-- ===============================================
            Display Report heading and subheading 
           ===============================================-->
        <b>
          <font size='3'>
             <center>Glossary Term<br />
             QC Report</center>
          </font>
        </b>
        <b>
          <font size='3'>
            <center>
             <xsl:value-of select="document('cdrutil:/date/%25B %25#d, %25Y %25#I:%25M%25p')"/>
         </center>
         </font>
         </b><br /><br />
         <b>
         <font size='3'>
         <xsl:apply-templates select="PDQKey"/>
         </font>
         </b><br />
         <b><xsl:value-of select="document('cdrutil:/docid')"/></b><br /><br />
            <xsl:if test="@Dictionary">
             <b>
               <font size='3'>Dictionary</font>
            </b>
                 <xsl:text>......................................</xsl:text>
             <xsl:apply-templates select="@Dictionary"/><br /><br />
           </xsl:if>

  <table width = '90%'>
    <tr>
      <td width = '25%'><b><font size='3'>Name</font></b></td>
      <td width = '5%'>&#xa0;</td>
      <td width = '70%'><xsl:apply-templates select="TermName"/></td>
    </tr>
    <tr>
      <td>
        &#xa0;
      </td>
    </tr>
    <tr>
      <xsl:if test="not(TermPronunciation)">
        <td><b><font size='3'>Pronunciation</font></b></td>
      </xsl:if>
      <xsl:if test="TermPronunciation">
        <td><b><font size='3'>Pronunciation</font></b></td>
        <td></td>
        <td><xsl:apply-templates select="TermPronunciation"/></td>
      </xsl:if>
    </tr>
  </table>           

  <br />
          
           <xsl:for-each select="TermDefinition">
            <xsl:choose>
            <xsl:when test="not(@Audience)">
           <b><font size='3'>Definition</font>
              </b><br />
            </xsl:when>
            <xsl:otherwise>
            <b><font size='3'>Definition[Health Professional]</font>
               </b><br />
            </xsl:otherwise>
            </xsl:choose>
            <xsl:apply-templates select="."/><br /><br />
            </xsl:for-each>
            
  <table width = '90%'>
    <tr>
      <td width = '25%'><b><font size='3'>Source</font></b></td>
      <td width = '5%'>&#xa0;</td>
      <td width = '70%'><xsl:apply-templates select="TermSource"/></td>
    </tr>
    <tr>
      <td>
        &#xa0;
      </td>
    </tr>
    <tr>
      <td><b><font size='3'>Type</font></b></td>
      <td></td>
      <td><xsl:apply-templates select="TermType"/></td>
    </tr>
    <tr>
      <td>
        &#xa0;
      </td>
    </tr>
    <tr>
      <td><b><font size='3'>Status</font></b></td>
      <td></td>
      <td><xsl:apply-templates select="TermStatus"/></td>
    </tr>
    <tr>
      <td>
        &#xa0;
      </td>
    </tr>
    <tr>
      <td><b><font size='3'>Date</font></b></td>
      <td></td>
      <td><xsl:apply-templates select="StatusDate"/></td>
    </tr>
    <tr>
      <td>
        &#xa0;
      </td>
    </tr>
<!--    <tr>
      <td><b><font size='3'>PDQ Key</font></b></td>
      <td></td>
      <td><xsl:apply-templates select="PdqKey"/></td>
    </tr> 
    <tr>
      <td>
        &#xa0;
      </td>
    </tr> -->
  </table>


  <xsl:if test="not(Comment)">
    <b><font size='3'>Comment</font></b>
  </xsl:if>
  <xsl:if test="Comment">
    <b><font size='3'>Comment&#xa0;&#xa0;&#xa0;</font></b>
    <xsl:apply-templates select="Comment"/>
  </xsl:if>
  <br /><br />

  <xsl:if test="not(DateLastModified)">
    <b><font size='3'>Date Last Modified</font></b>
    <br />
  </xsl:if>

  <xsl:if test="DateLastModified">
    <table width = '90%'>
      <tr>
        <td width = '25%'><b><font size='3'>Date Last Modified</font></b></td>
        <td width = '5%'>&#xa0;</td>
        <td width = '70%'><xsl:apply-templates select="DateLastModified"/></td>
      </tr>
    </table>
  </xsl:if>
      </BODY>  
   </xsl:template>

  </xsl:transform>]]>
</CdrDocXml>
</CdrDoc>