<CdrDocCtl>
<DocValStatus readonly="yes">U</DocValStatus>
<DocActiveStatus readonly="yes">A</DocActiveStatus>
<DocTitle readonly="yes">Module: InScopeProtocol HP Record Information</DocTitle>
<ReadyForReview readonly="yes">N</ReadyForReview>
</CdrDocCtl>
<CdrDocXml><![CDATA[<?xml version="1.0"?>
<xsl:transform             xmlns:xsl = "http://www.w3.org/1999/XSL/Transform"
                             version = "1.0"
                           xmlns:cdr = "cips.nci.nih.gov/cdr"
                           exclude-result-prefixes = "cdr">

   
 

  <!-- ===============================================================
              Display DateLastModified
     ================================================================ -->
  
          <xsl:template match="DateLastModified">
          <hr />
          <br />
          <table width='90%' cellpadding='0' cellspacing='0'>
          <tr>
          <th align='left' width='40%' valign='top'>   
          <b><font size='4'>Date Last Modified</font>
          </b>
          </th>
          <td width='60%'>
          <xsl:value-of     select="."/>
          </td>
          </tr>
          </table>
          </xsl:template>

  <!-- =====================================================================
             Display Record Information ( Date created, Modified, and User
     ===================================================================== --> 
              <xsl:template        match="CdrDocCtl">
              <br />
              <b><font size='4'>Record Information</font></b>
              <br />
              <table cellpadding='0' cellspacing ='0'  width='90%'>
              <tr>
              <th align='left' width='40%' valign='top'>   
              <b>
              <font size='3'>Created By
              </font>
              </b>
              </th>
               <td width='60%'>
              <xsl:value-of               select="Create/User"/>
              </td>
              </tr>
              <tr>
              <th align='left' width='40%' valign='top'>   
              <b>
              <font size='3'>Date
              </font>
              </b>
              </th>
              <td width='60%'>
              <xsl:value-of               select="substring-before 
                                                 (Create/Date,'T')"/>
             </td>
             </tr>
              <tr>
              <th align='left' width='40%' valign='top'>   
               <b>
              <font size='3'>Updated By
              </font>
              </b>
              </th>
              <td width='60%'>
              <xsl:value-of               select="Modify/User"/><br />
              </td>
              </tr>
               <tr>
              <th align='left' width='40%' valign='top'>   
              <b>
              <font size='3'>Date
              </font>
              </b>
              </th>
              <td width='60%'>
              <xsl:value-of               select="substring-before
                                                  (Modify/Date,'T')"/>
              </td>
              </tr>
              </table>
              <br /><br />
             </xsl:template>

             </xsl:transform>]]>
</CdrDocXml>
</CdrDoc>