<CdrDoc Type='Filter' Id='CDR0000327988'>
<CdrDocCtl>
<DocValStatus readonly="yes">U</DocValStatus>
<DocActiveStatus readonly="yes">A</DocActiveStatus>
<DocTitle readonly="yes">Organization Protocol Review Report Filter 2</DocTitle>
<ReadyForReview readonly="yes">N</ReadyForReview>
</CdrDocCtl>
<CdrDocXml><![CDATA[<?xml version="1.0"?>
<xsl:transform            xmlns:xsl = "http://www.w3.org/1999/XSL/Transform"
                          xmlns:cdr = "cips.nci.nih.gov/cdr"
                            version = "1.0">

 <xsl:output                 method = "html"/>

 <xsl:template                match = "/">
  <xsl:text disable-output-escaping = "yes"
   >&lt;!DOCTYPE HTML PUBLIC '-//IETF//DTD HTML//EN' /&gt;
</xsl:text>
  <xsl:apply-templates       select = "Organization"/>
 </xsl:template>

 <xsl:template                match = "Organization">
  <html>
   <head>
    <title>
     <xsl:value-of           select = "concat('@@DOC-ID@@ - ', OrgName)"/>
    </title>
    <style                     type = "text/css">
     body, td     { font-family: Arial, sans-serif; font-size: 12pt; }
    </style>
   </head>
   <body>
    <center>
     <span                    style = 'font-size: 18pt; font-weight: bold'>
      ORGANIZATION PROTOCOL REVIEW REPORT
     </span>
    </center>
    <hr />
    <span                     style = 'font-size: 14pt; font-weight: bold'>
     Name
    </span>
    <br />
    <br />
    <xsl:value-of            select = "OrgName"/>
    <br />
    <br />
    <span                     style = 'font-size: 14pt; font-weight: bold'>
     Locations
    </span>
    <br />
    <br />
    <xsl:if                    test = 'CipsContact'>
     <span                    style = 'font-size: 14pt'>
      CIPS Contact
     </span>
     <br />
     <xsl:apply-templates    select = 'CipsContact'/>
     <br />
     <br />
    </xsl:if>
    <xsl:if                    test = 'OtherLocation'>
     <span                    style = 'font-size: 14pt; font-weight: bold'>
      Other Locations
     </span>
     <br />
     <br />
     <xsl:apply-templates    select = 'OtherLocation'/>
    </xsl:if>
    <span                     style = 'font-size: 14pt; font-weight: bold'>
     Protocol List
    </span>
    <br /><br />
    @@TABLE@@
   </body>
  </html>
 </xsl:template>

 <xsl:template                match = 'CipsContact | OtherLocation'>
  <table                     border = '0'
                        cellspacing = '1'
                        cellpadding = '0'>
   <xsl:for-each             select = 'PostalAddress/Line'>
    <tr>
     <td                    colspan = '2'>
      <xsl:value-of          select = '.'/>
     </td>
    </tr>
   </xsl:for-each>
   <tr>
    <td                       width = '100'>Phone:&#160;&#160;&#160;</td>
    <td>
     <xsl:value-of           select = 'Phone'/>
    </td>
   </tr>
   <tr>
    <td>Fax:&#160;&#160;&#160;</td>
    <td>
     <xsl:value-of           select = 'Fax'/>
    </td>
   </tr>
   <tr>
    <td>Email:&#160;&#160;&#160;</td>
    <td>
     <xsl:value-of           select = 'Email'/>
    </td>
   </tr>
   <tr>
    <td>Website:&#160;&#160;&#160;</td>
    <td>
     <xsl:if                   test = 'WebSite'>
      <a                       href = '{WebSite}'>
<!--
       <xsl:attribute          name = 'href'>
        <xsl:value-of        select = 'WebSite'/>
       </xsl:attribute>
-->
       <xsl:value-of         select = 'WebSite'/>
      </a>
     </xsl:if>
    </td>
   </tr>
  </table>
  <br />
  <br />
 </xsl:template>

</xsl:transform>
]]>
</CdrDocXml>
</CdrDoc>