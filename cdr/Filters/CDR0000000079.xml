        </xsl:choose>
       </xsl:when>    
       <xsl:otherwise>
        <xsl:choose>
         <xsl:when                 test = "@existing">
          <xsl:text>]</xsl:text>
         </xsl:when>
         <xsl:otherwise>
          <xsl:choose>
           <xsl:when               test = "@RevisionLevel = 'proposed'">
            <span class="deleteproposed">
             <xsl:text>]</xsl:text>
            </span>
           </xsl:when>
           <xsl:otherwise>
            <span class="deleteapproved">
             <xsl:text>]</xsl:text>
            </span>
           </xsl:otherwise>
          </xsl:choose>
         </xsl:otherwise>
        </xsl:choose>
       </xsl:otherwise>
      </xsl:choose>
     </xsl:when>
     <xsl:otherwise>
      <xsl:text>]</xsl:text>
     </xsl:otherwise>
    </xsl:choose>
   </xsl:when>
  </xsl:choose>
 </xsl:template>


