<CdrDoc Type='Filter' Id='CDR0000343574'>
<CdrDocCtl>
<DocValStatus readonly="yes">U</DocValStatus>
<DocActiveStatus readonly="yes">A</DocActiveStatus>
<DocTitle readonly="yes">Module: InScopeProtocol HP Abstract</DocTitle>
<ReadyForReview readonly="yes">N</ReadyForReview>
</CdrDocCtl>
<CdrDocXml><![CDATA[<?xml      version = "1.0"?>
<!--
================================================================
$Id: CDR0000343574.xml,v 1.15 2008-01-11 22:48:21 venglisc Exp $

Filter module to process Protocol HP Abstract
Written by Cheryl Burg

$Log: not supported by cvs2svn $
================================================================ -->
<xsl:transform               xmlns:xsl = "http://www.w3.org/1999/XSL/Transform"
                               version = "1.0"
                             xmlns:cdr = "cips.nci.nih.gov/cdr"
               exclude-result-prefixes = "cdr">

 <xsl:include href = "cdr:name:Module: InScopeProtocol HP Eligibility"/>

 <!-- 
 =====================================================================
 Display Protocol HP Abstract
 ==================================================================== -->
 <xsl:template                   match = "ProtocolAbstract">
  <hr />
  <b>
   <font size='5'>
    <xsl:text>Professional Abstract</xsl:text>
   </font>
  </b>
  <br />
  <br />
  <xsl:apply-templates/>

  <p/>
 </xsl:template>


 <!-- 
 =====================================================================
 Display Protocol Objectives
 ==================================================================== -->
 <xsl:template                   match = "Objectives">
  <span class="big">Objectives</span>
  <p/>
  <xsl:apply-templates/>
  <p/>
 </xsl:template>


 <!-- 
 ====================================================================
 Display the Disease characteristics, Patient characteristics,
 Prior concurrent therapy, Eligibility Criteria
 ==================================================================== -->
 <xsl:template                   match = "DiseaseCharacteristics">
  <span class="big">Entry Criteria</span>
  <br />
  <br />
  <span class="big">Disease Characteristics:</span> 
  <br />
  <xsl:apply-templates/> 
  <p/>
 </xsl:template>

  
 <!-- 
 ================================================================
 Display the Patient Characteristics
 ================================================================ -->
 <xsl:template                   match = "PatientCharacteristics">
  <span class="big">Patient Characteristics:</span>
  <br />
  <xsl:apply-templates/>
  <p/>
 </xsl:template>


 <!-- 
 ================================================================
 Display the Prior Concurrent Therapy
 ================================================================ -->
 <xsl:template                  match = "PriorConcurrentTherapy">
  <span class="big">Prior Concurrent Therapy</span>
  <br />
  <xsl:apply-templates/>
  <p/>
 </xsl:template>
  
  
 <!-- 
 ================================================================
 Display the General Eligibility Criteria
 ================================================================ -->
 <xsl:template                   match = "GeneralEligibilityCriteria">
  <span class="big">General Eligibility Criteria:</span>
  <br />
  <br />
  <xsl:apply-templates/>
  <p/>
 </xsl:template>


 <!-- 
 ================================================================
 Display the projected accrual
 ================================================================ -->
 <xsl:template                   match = "ProjectedAccrual">
  <span class="big">Projected Accrual</span>
  <xsl:apply-templates/>
  <p/>
 </xsl:template>


 <!-- 
 ===============================================================
 Display Outcome
 =============================================================== -->
 <xsl:template                   match = "ProtocolAbstract/
                                          Professional/
                                          Outcomes">
  <span class="big">Outcome</span>
  <p/>

  <b>Primary</b>
  <br/>
  <ul>
   <xsl:apply-templates         select = "Outcome[@OutcomeType = 'Primary']"/>
  </ul>
  <p/>

  <xsl:if                         test = "Outcome[@OutcomeType = 'Secondary']">
   <b>Secondary</b>
   <br/>
   <ul>
    <xsl:apply-templates        select = "Outcome[@OutcomeType = 'Secondary']"/>
   </ul>
   <p/>
  </xsl:if>
 </xsl:template>


 <!-- 
 ===============================================================
 Display Outcome items
 =============================================================== -->
 <xsl:template                   match = "Outcome">
  <li>
   <xsl:apply-templates/>
  </li>
 </xsl:template>


 <!-- 
 ===============================================================
 Display Outline
 =============================================================== -->
 <xsl:template                   match = "ProtocolAbstract/
                                          Professional/
                                          Outline">
  <span class="big">Outline</span>
  <xsl:apply-templates/>
 </xsl:template>


 <!-- 
 ===================================================================
 Display End Points
 =================================================================== -->
 <xsl:template                   match = "ProtocolAbstract/
                                          Professional/
                                          EndPoints">
  <span class="big">End Points</span>
  <br />
  <br />
  <xsl:apply-templates/>
  <p/>
 </xsl:template>


 <!-- 
 ===================================================================
 Display Stratification
 =================================================================== -->
 <xsl:template                   match = "ProtocolAbstract/
                                          Professional/
                                          Stratification">
  <span class="big">Stratification</span>
  <br />
  <br />
  <xsl:apply-templates/>
  <p/>
 </xsl:template>


 <!-- 
 ===================================================================
 Display Special Study Parameters
 =================================================================== -->
 <xsl:template                   match = "ProtocolAbstract/
                                          Professional/
                                          SpecialStudyParameters">
  <span class="big">Special Study Parameters</span>
  <br />
  <br />
  <xsl:apply-templates/>
  <p/>
 </xsl:template>


 <!-- 
 ===================================================================
 Display Dosage Form
 =================================================================== -->
 <xsl:template                   match = "ProtocolAbstract/
                                          Professional/
                                          DosageForm">
  <span class="big">Dosage Form</span>
  <br />
  <br />
  <xsl:apply-templates/>
  <p/>
 </xsl:template>


 <!-- 
 ===================================================================
 Display Dose Schedule
 =================================================================== -->
 <xsl:template                   match = "ProtocolAbstract/
                                          Professional/
                                          DoseSchedule">
  <span class="big">Dosage Schedule</span>
  <xsl:apply-templates/>
  <p/>
 </xsl:template>

 <!-- 
 ===================================================================
 Do not display the Comment
 =================================================================== -->
 <xsl:template                   match = "Comment">

  <xsl:variable                   name = "type">
   <xsl:choose>
    <xsl:when                      test = "name(..) = 'Professional'">
     <xsl:text>HP Abstract</xsl:text>
    </xsl:when>
    <xsl:when                      test = "name(..) = 'Patient'">
     <xsl:text>Pat Abstract</xsl:text>
    </xsl:when>
    <xsl:otherwise>
     <xsl:value-of               select = "name(..)"/>
     <!-- xsl:value-of               select = "substring(name(..), 1, 15)"/ -->
    </xsl:otherwise>
   </xsl:choose>
  </xsl:variable>

  <table        xsl:use-attribute-sets = "table">
   <xsl:apply-templates         select = "."
                                  mode = "withAttribute">
    <xsl:with-param               name = "label"
                                select = "concat($type, ' Comment')"/>
   </xsl:apply-templates>
  </table>
  <p/>
 </xsl:template>

</xsl:transform>]]>
</CdrDocXml>
</CdrDoc>