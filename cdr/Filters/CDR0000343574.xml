<CdrDocXml><![CDATA[<?xml      version = "1.0"?>
<!--
 ================================================================
     $Id: CDR0000343574.xml,v 1.6 2004-08-04 20:55:38 venglisc Exp $

     Filter module to process Protocol HP Abstract
        Written by Cheryl Burg

     $Log: not supported by cvs2svn $
 ================================================================ --><xsl:transform               xmlns:xsl = "http://www.w3.org/1999/XSL/Transform"
                               version = "1.0"
                             xmlns:cdr = "cips.nci.nih.gov/cdr"
               exclude-result-prefixes = "cdr">

 <!-- 
 =====================================================================
 Display Protocol HP Abstract
 ==================================================================== -->
 <xsl:template                   match = "ProtocolAbstract">
  <hr />
  <b>
   <font size='5'>
    <xsl:text>Professional Abstract</xsl:text>
   </font>
  </b>
  <br />
  <br />
  <xsl:apply-templates/>

  <table        xsl:use-attribute-sets = "table">
   <tr>
    <td         xsl:use-attribute-sets = "cell1of2">
     <b>Comment</b>
    </td>
    <td         xsl:use-attribute-sets = "cell2of2">
    <xsl:value-of               select = "Professional/
                                          Comment"/>
    </td>
   </tr>
  </table>
  <p/>
 </xsl:template>


 <!-- 
 =====================================================================
 Display Protocol Objectives
 ==================================================================== -->
 <xsl:template                   match = "Objectives">
  <b>
   <font size='4'>Objectives</font>
  </b>
  <br />
  <br />
  <xsl:apply-templates/>
  <p/>
 </xsl:template>


 <!-- 
 ====================================================================
 Display the Disease characteristics, Patient characteristics,
 Prior concurrent therapy, Eligibility Criteria
 ==================================================================== -->
 <xsl:template                   match = "DiseaseCharacteristics">
  <b>
   <font size='4'>Entry Criteria</font>
  </b>
  <b>
  <br />
  <br />
   <font size='4'>Disease Characteristics:</font> 
  </b>
  <br />
  <xsl:apply-templates/>
  <p/>
 </xsl:template>

  
 <!-- 
 ================================================================
 Display the Patient Characteristics
 ================================================================ -->
 <xsl:template                   match = "PatientCharacteristics">
  <b>
   <font size='4'>Patient Characteristics:</font>
  </b>
  <br />
  <xsl:apply-templates/>
  <p/>
 </xsl:template>


 <!-- 
 ================================================================
 Display the Prior Concurrent Therapy
 ================================================================ -->
 <xsl:template                  match = "PriorConcurrentTherapy">
  <b>
   <font size='4'>Prior Concurrent Therapy</font>
  </b>
  <br />
  <xsl:apply-templates/>
  <p/>
 </xsl:template>
  
  
 <!-- 
 ================================================================
 Display the General Eligibility Criteria
 ================================================================ -->
 <xsl:template                   match = "GeneralEligibilityCriteria">
  <b>
   <font size='4'>General Eligibility Criteria:</font>
  </b>
  <br />
  <br />
  <xsl:apply-templates/>
  <p/>
 </xsl:template>


 <!-- 
 ================================================================
 Display the projected accrual
 ================================================================ -->
 <xsl:template                   match = "ProjectedAccrual">
  <b>
   <font size='4'>Projected Accrual</font>
  </b>
  <xsl:apply-templates/>
  <p/>
 </xsl:template>


 <!-- 
 ===============================================================
 Display Outline
 =============================================================== -->
 <xsl:template                   match = "ProtocolAbstract/
                                          Professional/
                                          Outline">
  <b>
   <font size='4'>Outline</font>
  </b>
  <xsl:apply-templates/>
 </xsl:template>


 <!-- 
 ===================================================================
 Display End Points
 =================================================================== -->
 <xsl:template                   match = "ProtocolAbstract/
                                          Professional/
                                          EndPoints">
  <b>
   <font size='4'>End Points</font>
  </b>
  <br />
  <br />
  <xsl:apply-templates/>
  <p/>
 </xsl:template>


 <!-- 
 ===================================================================
 Display Stratification
 =================================================================== -->
 <xsl:template                   match = "ProtocolAbstract/
                                          Professional/
                                          Stratification">
  <b>
   <font size='4'>Stratification</font>
  </b>
  <br />
  <br />
  <xsl:apply-templates/>
  <p/>
 </xsl:template>


 <!-- 
 ===================================================================
 Display Special Study Parameters
 =================================================================== -->
 <xsl:template                   match = "ProtocolAbstract/
                                          Professional/
                                          SpecialStudyParameters">
  <b>
   <font size='4'>Special Study Parameters</font>
  </b>
  <br />
  <br />
  <xsl:apply-templates/>
  <p/>
 </xsl:template>


 <!-- 
 ===================================================================
 Display Dosage Form
 =================================================================== -->
 <xsl:template                   match = "ProtocolAbstract/
                                          Professional/
                                          DosageForm">
  <b>
   <font size='4'>Dosage Form
  </font>
  </b>
  <br />
  <br />
  <xsl:apply-templates/>
  <p/>
 </xsl:template>


 <!-- 
 ===================================================================
 Display Dose Schedule
 =================================================================== -->
 <xsl:template                   match = "ProtocolAbstract/
                                          Professional/
                                          DoseSchedule">
  <b>
   <font size='4'>Dosage Schedule</font>
  </b>
  <xsl:apply-templates/>
  <p/>
 </xsl:template>

</xsl:transform>]]>
</CdrDocXml>
</CdrDoc>