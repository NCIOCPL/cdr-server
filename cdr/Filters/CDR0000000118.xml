<CdrDoc Type='Filter' Id='CDR0000000118'>
<CdrDocCtl>
<DocValStatus readonly="yes">U</DocValStatus>
<DocActiveStatus readonly="yes">A</DocActiveStatus>
<DocTitle readonly="yes">Political SubUnit QC Report</DocTitle>
<DocComment>CVS-V1.6: Add CDR ID (Bug 833)</DocComment><ReadyForReview readonly="yes">N</ReadyForReview>
</CdrDocCtl>
<CdrDocXml><![CDATA[<?xml version="1.0"?>
  <xsl:transform xmlns:xsl="http://www.w3.org/1999/XSL/Transform"
                 xmlns:cdr="cips.nci.nih.gov/cdr"
                 version="1.0">
   <xsl:output method="html"/>

   <xsl:template match="/">
    <html>
     <head>
       <xsl:variable                name="cdrmydoc" 
                                     select="document('cdrx:/*/CdrCtl')"/>
      
     <xsl:variable      name = "docID"
                                select = "document('cdrutil:/docid')"/>
 
            <title>
            <xsl:value-of   select = "concat('CDR', 
                                   number(
                                   substring-after($docID, 'CDR')))"/>
           <xsl:text>: </xsl:text>                        
           <xsl:value-of  select="$cdrmydoc/CdrDocCtl/DocTitle"/>
                                                          
                                                                                                                                      
                                                                                                                                           
            </title>

     </head>
     <basefont face='Arial, Helvetica, sans-serif' />
     <xsl:apply-templates select="PoliticalSubUnit"/>
    </html>
   </xsl:template>

   <xsl:template match="PoliticalSubUnit">
    <body>
     <center>
      <b>
       <font size='4'>Political SubUnit<br />QC Report</font>
      </b>
       <br />
      <b>
      <font size='3'>
      <xsl:value-of        select="document('cdrutil:/date/%25B %25#d, %25Y %
                                   25#I:%25M% 25p')"/>
     </font>
     </b>
     </center>
     <br /><br />
      <xsl:variable                name = "docID"
                                   select = "document('cdrutil:/docid')"/>
     <b>
     <font size='4'>
     <xsl:value-of              select = "concat('CDR',number( 
                                          substring-after($docID,'CDR')))"/>

   </font>
   </b>
   <br />
   <br />

     <table  cellpadding='0' cellspacing='0' width='90%' border ='0'>
       <tr>
       <th align='left' width='40%' nowrap='1'>
       <xsl:text>Full Name:</xsl:text>
       </th>
       <td width='60%'>
       <xsl:value-of     select="PoliticalSubUnitFullName"/>
       </td>
       </tr>
      <xsl:for-each      select="PoliticalSubUnitShortName">
       <tr>
        <th align='left' width='40%' nowrap='1'>
        <xsl:if          test= "not(preceding-sibling::
                                PoliticalSubUnitShortName)">   
       <xsl:text>Short Name:</xsl:text> 
       </xsl:if>
       </th>
        
      <td>
      <xsl:value-of  select="."/>
      </td>
      </tr>
      </xsl:for-each>
      <xsl:for-each select="PoliticalSubUnitAlternateName">
       <tr>
        <th align='left' width='40%' nowrap='1'>
         <xsl:if         test= "not(preceding-sibling::
                                PoliticalSubUnitAlternateName)">   
        
         <xsl:text>Alternate Name:</xsl:text> 
         </xsl:if>
         </th>
        
        <td>
        <xsl:value-of    select="."/>
        </td>
        </tr>
       </xsl:for-each>
      <tr>
      <th align='left' width='40%'>
      <xsl:text>Country:</xsl:text> 
      </th>
       <td>
       <xsl:value-of      select="Country/CountryFullName"/>
       <xsl:text>&#xa0;&#xa0;(</xsl:text>
       <xsl:value-of      select="concat('CDR', number(substring-after(
                                  Country/@cdr:ref,'CDR')))"/>
       <xsl:text>) </xsl:text>
       </td>
       </tr>
       <xsl:for-each       select="DateLastModified">
       <tr>
        <th align='left' nowrap='1'>
        <xsl:text>Date Last Modified:</xsl:text> 
        </th>
        <td>
        <xsl:value-of  select="."/>
       </td>
       </tr>
      </xsl:for-each>
     </table>
    </body>
   </xsl:template>

  </xsl:transform>]]>
</CdrDocXml>
</CdrDoc>