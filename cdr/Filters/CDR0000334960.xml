<CdrDoc Type='Filter' Id='CDR0000334960'>
<CdrDocCtl>
<DocValStatus readonly="yes">U</DocValStatus>
<DocActiveStatus readonly="yes">A</DocActiveStatus>
<DocTitle readonly="yes">Glossary Term Display for HPS Summaries</DocTitle>
<ReadyForReview readonly="yes">N</ReadyForReview>
</CdrDocCtl>
<CdrDocXml><![CDATA[<?xml version="1.0"?>
  <xsl:transform xmlns:xsl="http://www.w3.org/1999/XSL/Transform"
                 version="1.0">
   <xsl:output method="html"/>
   <xsl:include    href = "cdr:name:InLine Markup Formatter"/>

  <!-- Written by Nanci, 5/14/2002, modified cb 9/3/2003 -->

   <!--=============================================================
         Displays Glossary Term, Pronunciation, and Definition
       =============================================================-->

   <xsl:template                match="GlossaryTerm">
    <xsl:value-of               select="SpanishTermName"/>
     <xsl:if                    test="TermPronunciation">
     <xsl:text>&#xa0;&#xa0;</xsl:text>
     <xsl:value-of              select="TermPronunciation"/>
     </xsl:if>
     <br /><br />
     <xsl:for-each             select = "SpanishTermDefinition">
     <xsl:if                   test ="Audience = 'Health professional'">
     
     <xsl:apply-templates            select="DefinitionText"/>
     </xsl:if>
     </xsl:for-each>
   </xsl:template>

  </xsl:transform>]]>
</CdrDocXml>
</CdrDoc>