<?xml version="1.0"?>
<!--
     $Id: CDR0000000113.xml,v 1.1.1.1 2002-07-01 15:34:22 bkline Exp $

     Filter which sets the status of the Protocol.

     $Log: not supported by cvs2svn $
     Revision 1.1  2002/02/07 13:10:22  bkline
     Initial revision

  -->

<xsl:transform       xmlns:xsl = "http://www.w3.org/1999/XSL/Transform"
                     xmlns:cdr = "cips.nci.nih.gov/cdr"
                       version = "1.0">

 <xsl:param               name = "newStatus"/>

 <!-- Identity rule, copies everything to the output tree. -->
 <xsl:template           match = "@*|node()">
  <xsl:copy>
   <xsl:apply-templates select = "@*|node()"/>
  </xsl:copy>
 </xsl:template>

 <!-- This is the only specific template in the script. -->
 <xsl:template           match = "ProtocolAdminInfo/CurrentProtocolStatus">
  <CurrentProtocolStatus>
   <xsl:value-of        select = "$newStatus"/>
  </CurrentProtocolStatus>
 </xsl:template>

</xsl:transform>