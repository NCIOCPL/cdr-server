<CdrDoc Type='Filter' Id='CDR0000343578'>
<CdrDocCtl>
<DocValStatus readonly="yes">U</DocValStatus>
<DocActiveStatus readonly="yes">A</DocActiveStatus>
<DocTitle readonly="yes">Module: InScopeProtocol Patient Abstract</DocTitle>
<ReadyForReview readonly="yes">N</ReadyForReview>
</CdrDocCtl>
<CdrDocXml><![CDATA[<?xml      version = "1.0"?>
<!--
 ================================================================
     $Id: CDR0000343578.xml,v 1.7 2004-08-31 20:02:44 venglisc Exp $

     Filter to Process Protocol Patient Abstract
        Written by Volker Englisch, 2004-08-01

     $Log: not supported by cvs2svn $
 ================================================================ -->
<xsl:transform               xmlns:xsl = "http://www.w3.org/1999/XSL/Transform"
                               version = "1.0"
                             xmlns:cdr = "cips.nci.nih.gov/cdr"
               exclude-result-prefixes = "cdr">
  
 <!--
 ===============================================================
 Display Protocol Professional or Patient Title
 ================================================================ -->
 <xsl:template                   match = "ProtocolTitle[@Type='Patient']">
  <hr />
  <b>
   <font size='5'>Patient Abstract</font>
  </b>
  <p/>
  <b>
   <font size='4'>Patient Title</font>
  </b>
  <br/>
  <xsl:apply-templates/>
  <p/>
 </xsl:template>

 <!-- 
 ==================================================================
 Display the Patient Purpose, Eligibility, Treatment Intervention
 ================================================================== -->  
 <xsl:template                   match = "Rationale">
  <b>Rationale</b>

  <xsl:apply-templates/>
 </xsl:template>


 <!-- 
 ==================================================================
 Display the Patient Purpose, Eligibility, Treatment Intervention
 ================================================================== -->  
 <xsl:template                   match = "Purpose">
  <b>Purpose</b>

  <xsl:apply-templates/>
 </xsl:template>


 <!-- 
 ==================================================================
 Display the Patient Purpose, Eligibility, Treatment Intervention
 ================================================================== -->  
 <xsl:template                   match = "EligibilityText">
  <b>Eligibility</b>
  <p/>
  <xsl:apply-templates/>
 </xsl:template>


 <!-- 
 ==================================================================
 Display the Patient Purpose, Eligibility, Treatment Intervention
 ================================================================== -->  
 <xsl:template                   match = "TreatmentIntervention">
  <b>Treatment Intervention</b>

  <xsl:apply-templates/>

  <table        xsl:use-attribute-sets = "table">
   <tr>
    <td         xsl:use-attribute-sets = "cell1of2">
     <b>Entered By</b>
    </td>
    <td         xsl:use-attribute-sets = "cell2of2">
     <xsl:value-of    select="../EnteredBy"/>
    </td>
   </tr>
   <tr>
    <td         xsl:use-attribute-sets = "cell1of2">
     <b>Entry Date</b>
    </td>
    <td         xsl:use-attribute-sets = "cell2of2">
     <xsl:value-of      select="../EntryDate"/>
    </td>
   </tr>
  </table>
 </xsl:template>

 
 <!-- 
 ====================================================================
 Do not display EnteredBy or EntryDate
 ==================================================================== -->
 <xsl:template                   match = "EnteredBy | 
                                          EntryDate | 
                                          PatientDisclaimer"/>
</xsl:transform>]]>
</CdrDocXml>
</CdrDoc>