<?xml   version = "1.0"?>
<xsl:transform            xmlns:xsl = "http://www.w3.org/1999/XSL/Transform"
                            version = "1.0"
                          xmlns:cdr = "cips.nci.nih.gov/cdr">

 <xsl:output                 method = "html"/>

 <xsl:template                match = "/">
  <xsl:text disable-output-escaping = "yes">
   &lt;!DOCTYPE HTML PUBLIC '-//IETF//DTD HTML//EN' /&gt;
  </xsl:text>

  <html>
   <head>
    <title>Demo of href links</title>
   </head>
   <body>
    <h2>Hi!  Here are some glossary terms...</h2>
    <ol>
     <xsl:apply-templates    select = "//GlossaryTermRef"/>
    </ol>
   </body>
  </html>
 </xsl:template>

 <xsl:template                match = "GlossaryTermRef">
  <li>
   <a>
    <xsl:attribute             name = "href">
     <xsl:value-of           select = "concat('/cgi-bin/cdr/Filter.py?DocId=',
                                              @cdr:href,
                                              '&amp;Filter=name:Glossary+',
                                              'Term+QC+Report+filter')"/>
    </xsl:attribute>
    <xsl:value-of            select = "."/>
   </a>
  </li>
 </xsl:template>

 </xsl:transform>
