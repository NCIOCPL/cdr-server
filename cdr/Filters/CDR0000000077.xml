<?xml version="1.0"?>
 <xsl:transform xmlns:xsl="http://www.w3.org/1999/XSL/Transform"
                version="1.0"
                xmlns:cdr="cips.nci.nih.gov/cdr">
  <xsl:output method="html"/>

  <xsl:template match="/">
   <!-- <!DOCTYPE HTML PUBLIC '-//IETF//DTD HTML//EN' /> -->
   <HTML>
    <HEAD> <!-- LINE 10 -->
     <TITLE>@@DOCID@@</TITLE>
     <!-- META HTTP-EQUIV="Pragma" CONTENT="no-cache" / -->
     <LINK REL='stylesheet' 
           HREF='/stylesheets/term.css' 
           TYPE='text/css' />
    </HEAD>
    <BASEFONT FACE='Arial, Helvetica, sans-serif' />
    <xsl:apply-templates select="GeographicEntity"/>
   </HTML>
  </xsl:template> <!-- LINE 20 -->

  <xsl:template match='GeographicEntity'>
   <BODY>
    <HR />
    <H1>Geographic Entity Document @@DOCID@@</H1>
    <HR />
    <H2><xsl:value-of select="CountryFullName"/></H2>
    <TABLE>
     <xsl:for-each select="CountryFullName">
      <TR>
       <TH ALIGN='right' NOWRAP='1'>Country Full Name:</TH>
       <TD><xsl:value-of select="."/></TD>
      </TR>
     </xsl:for-each>
     <xsl:for-each select="CountryShortName">
      <TR>
       <TH ALIGN='right' NOWRAP='1'>Country Short Name:</TH>
       <TD><xsl:value-of select="."/></TD>
      </TR>
     </xsl:for-each>
     <xsl:for-each select="CountryAlternateName">
      <TR>
       <TH ALIGN='right' NOWRAP='1'>Country Alternate Name:</TH>
       <TD><xsl:value-of select="."/></TD>
      </TR>
     </xsl:for-each>
     <xsl:for-each select="PoliticalUnit/PoliticalUnitFullName">
      <TR>
       <TH ALIGN='right' NOWRAP='1'>Political Unit:</TH>
       <TD><xsl:value-of select="."/></TD>
      </TR>
     </xsl:for-each>
    </TABLE>
   </BODY>
  </xsl:template>

 </xsl:transform>