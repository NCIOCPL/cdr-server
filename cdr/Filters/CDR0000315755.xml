      <xsl:value-of select = "PostalAddress/PostalCode_ZIP"/>
      <br />
      <br />
      <xsl:value-of select = "PostalAddress/CountryName"/>
    </xsl:if>  

    <xsl:value-of select = "concat('Phone:&#xa0;&#xa0;&#xa0;&#xa0;
