<CdrDoc Type='Filter' Id='CDR0000343573'>
<CdrDocCtl>
<DocValStatus readonly="yes">U</DocValStatus>
<DocActiveStatus readonly="yes">A</DocActiveStatus>
<DocTitle readonly="yes">Module: HP InScopeProtocol Details</DocTitle>
<ReadyForReview readonly="yes">N</ReadyForReview>
</CdrDocCtl>
<CdrDocXml><![CDATA[<?xml      version = "1.0"?>
<xsl:transform               xmlns:xsl = "http://www.w3.org/1999/XSL/Transform"
                               version = "1.0"
                             xmlns:cdr = "cips.nci.nih.gov/cdr"
               exclude-result-prefixes = "cdr">
 
 <!-- 
 ===================================================================
 Display Protocol Details
 =================================================================== -->
 <xsl:template                   match = "ProtocolDetail">
  <hr />
  <b>
   <font size='4'>Protocol Details</font>
  </b>
  <table width='90%' cellpadding='0' cellspacing='0'>
   <tr>
   <th align='left' width='40%' valign='top'>
   <b><xsl:text>Study Type</xsl:text>
   </b>
   </th>
   <td width='60%'>
   <xsl:value-of          select="StudyType"/>
   </td>
   </tr>
   </table>
    <table width='90%' cellpadding='0' cellspacing='0'>
    <xsl:for-each            select="StudyCategory">
    <tr>
   <th align='left' width='40%' valign='top'>
   <b>
   <xsl:text>Information Category Type</xsl:text>
   </b>
   </th>
      <td width='60%'>
   <xsl:value-of            select="StudyCategoryType"/>
   </td>
   </tr>
   <tr>
   <th align='left' width='40%' valign='top'>
   <b><xsl:text>Information Category Name</xsl:text></b>
   </th>
   <td width='60%'>
   <xsl:value-of           select="StudyCategoryName"/>
   </td>
   </tr>
   <tr>
   <th align='left' width='40%' valign='top'>
   <b>
   <xsl:text>Study Focus</xsl:text>
  </b>
  </th>

  <td width='60%'>
  <xsl:value-of               select="StudyFocus"/>
  </td>
  </tr>

  <xsl:apply-templates          select = "Intervention"/>

    </xsl:for-each>
    </table>
    <b>
   <font size='3'>
   <xsl:text>Comment</xsl:text>
   </font>
   </b>
   <br />
   <xsl:for-each      select="StudyCategory/Comment">
   <xsl:value-of      select="."/>
   </xsl:for-each>
   <xsl:if           test="StudyCategory/Comment">
   <br />
   <br />
   </xsl:if>
      
   <table width='90%' cellpadding='0' cellspacing='0'>
   <tr>
   <td align='left' width='40%' valign='top'>
   <b>
   <font size='3'>Condition
   </font>
   </b>
   </td>
    <td>
  <xsl:for-each select="Condition/Term/PreferredName">
    <table width='90%' cellpadding='0' cellspacing='0'>
    <tr>
    <td width='60%'>  
    <xsl:sort                 select="."/>
    <xsl:value-of             select="."/>
    </td>
    </tr>
    </table>
   </xsl:for-each>
   </td>
   </tr>
   </table>
   <br />     

    

    <table width='90%' cellpadding='0' cellspacing='0'>
     <tr>
     <td align='left' width='40%' valign='top'>
     <b><xsl:text>Gene 
     </xsl:text>
     </b>
     </td>
     <td>
    <xsl:for-each        select="Gene">
     <table width='90%' cellpadding='0' cellspacing='0'>
    <tr>
    <td width='60%'>
    <xsl:value-of             select="Term/PreferredName"/>
    </td>
    </tr>
    </table>
    </xsl:for-each>
    </td>
    </tr>
    </table>
    <br />
 
   <table width='90%' cellpadding='0' cellspacing='0'>
   <tr>
   <td width='40%'>
   <b><font size='3'>Entered By</font>
   </b>
   </td> 
   <td width='60%'>
   <xsl:value-of          select="EnteredBy"/> 
   </td>
   </tr>
  <tr>
  <td width='40%'>
  <b>
  <font size='3'>Entry Date
  </font>
  </b>
  </td>
  <td  width='60%'>
  <xsl:value-of           select="EntryDate"/>
   </td>
   </tr>
  </table>
  </xsl:template>


 <!--
 ===================================================================
 =================================================================== -->
 <xsl:template                   match = "Intervention">
  <xsl:apply-templates          select = "InterventionType"/>
  <xsl:choose>
   <xsl:when                      test = "InterventionNameLink">
    <xsl:apply-templates        select = "InterventionNameLink"/>
   </xsl:when>
   <xsl:otherwise>
    <tr>
     <td><xsl:text> </xsl:text></td>
     <td><br/></td>
    </tr>
   </xsl:otherwise>
  </xsl:choose>
 </xsl:template>


 <!--
 ===================================================================
 =================================================================== -->
 <xsl:template                   match = "InterventionType">
  <tr>
   <td          xsl:use-attribute-sets = "cell1of2">
    <b>Intervention Type</b>
   </td>
   <td          xsl:use-attribute-sets = "cell2of2">
    <xsl:value-of               select = "Term/PreferredName"/>
   </td>
  </tr>
 </xsl:template>


 <!--
 ===================================================================
 =================================================================== -->
 <xsl:template                   match = "InterventionNameLink">
  <tr>
   <td          xsl:use-attribute-sets = "cell1of2">
    <b>Intervention Name</b>
   </td>
   <td          xsl:use-attribute-sets = "cell2of2">
    <table      xsl:use-attribute-sets = "table">
     <tr>
      <td       xsl:use-attribute-sets = "cell1of2">
       <ul class='term'>
        <xsl:for-each           select = "Term/PreferredName">
         <xsl:sort              select = "."/>
         <li>
          <xsl:value-of         select = "translate(.,'/','-')"/>
         </li>
        </xsl:for-each>
       </ul>
      </td>
     </tr>
    </table>
   </td>
  </tr>
 </xsl:template>



<!-- ================================================================
                  Display Eligibilty Information
    ================================================================= -->

  <xsl:template         match="Eligibility">
  <br />
  <b>
  <font size='4'>Eligibility Criteria
  </font>
  </b>
  <br />
  <table width='90%' cellpadding='0' cellspacing='0'>
   <tr>
   <th align='left' width='40%' valign='top'>
   <b>
  <font size='3'>Healthy Volunteers?
  </font>
  </b>
  </th>
  <td width='60%'>
  <xsl:apply-templates     select="HealthyVolunteers"/>
  </td>
  </tr>
  
   <tr>
   <th align='left' width='40%' valign='top'>
   <b>
   <font size='3'>Low Age</font>
  </b>
  </th>
  <td width='60%'>
  <xsl:apply-templates     select="LowAge"/>
  </td>
  </tr>
 
  <tr>
  <th align='left' width='40%' valign='top'>
  <b>
  <font size='3'>High Age
  </font>
  </b>
  </th>
  <td width='60%'>
   <xsl:apply-templates         select="HighAge"/>
   </td>
   </tr>
    <tr>
   <th align='left' width='40%' valign='top'>
   <b>
  <font size='3'>Age Range</font>
  </b>
  </th>
  
  
  <td width='60%'>
  <xsl:apply-templates      select="AgeText"/>
  </td>
 </tr> 
  </table> 
  
  
  <table cellpadding='0' cellspacing='0' width='90%'>
   <tr>
   <th align='left' width='40%' valign='top'>
   <b>
   <font size='3'>Diagnosis
   </font>
   </b>
   </th>
   <td>
   <table cellpadding='0' cellspacing='0' width='90%'>
   <tr>
   <td width='60%'>
   <ul class='term'>
   <xsl:for-each             select="Diagnosis/Term/PreferredName">
     <li>
   <xsl:sort                 select="."/>
   <xsl:value-of             select="."/>
     </li>
   </xsl:for-each>
     </ul>
     </td>
     </tr>
     </table>
     </td>
     </tr>
    
   </table>
      
   <b>
   <font size='3'>
   <xsl:text>Comment</xsl:text>
   </font>
   </b>
    <br />
   <xsl:value-of          select="Comment"/>
   <xsl:if                test="Comment">
   <br />
   <br />
   </xsl:if>
      
   <table cellpadding='0' cellspacing='0' width='90%'>
   <tr>
   <td align='left' width='40%' valign='top'>
   <b>
   <font size='3'>Exclusion Criteria
   </font>
   </b>
   </td>
   <td>
   <table width='90%' cellpadding='0' cellspacing='0' >
    <tr>
    <td width='60%'> 
    <ul class='term'>
    <xsl:for-each             select="ExclusionCriteria/Term/PreferredName">
    <xsl:sort                select="."/>
     <li>
     <xsl:value-of             select="."/>
     </li>
    </xsl:for-each>
     </ul>
     </td>
     </tr>
     </table>
     </td>
     </tr>
    </table>
   </xsl:template>

<!-- ===============================================================
              Display Published Results
     ================================================================ -->
  
          <xsl:template     match="PublishedResults">
          <xsl:if           test="not(preceding-sibling::PublishedResults)">
           <br />
          <b><font size='4'>Published Results</font></b>
           <br />
           <br />
          </xsl:if>
         <xsl:apply-templates select="." mode="copy"/>
          <br />
          <br />
         </xsl:template>



   <!-- =============================================================
               Display Protocol Special Category
       ============================================================ -->

              <xsl:template   match = "ProtocolSpecialCategory">
              <xsl:if         test="not(preceding-sibling::
                                    ProtocolSpecialCategory)">
              <b>
              <font size='4'>
              <xsl:text>Protocol Special Category</xsl:text>
              </font>
              </b>
              </xsl:if>
              <table width='90%'  cellpadding='0' cellspacing='0'> 
              <tr>
              <th align='left' width='40%'>   
              <u>
              <b><font size='3'>Special Category
              </font>
              </b>
              </u>
              </th>
              <td width='60%'>
              <xsl:value-of      select="SpecialCategory"/>
                                                      
              </td>
              </tr>
              </table>
              <b><font size='3'>Comment
              </font>
              </b>
               <br />
              <xsl:value-of          select="Comment"/>
              <br />
              <br />
             </xsl:template>

<!-- ===============================================================
              Display Protocol-Related Links
     ================================================================ -->
  
       <xsl:template             match = "ProtocolRelatedLinks">
         <b><font size='4'>Related Protocols
         </font>
         </b>
         <br />
         <xsl:for-each              select="RelatedProtocols/InScopeProtocol">
          <xsl:value-of              select="ProtocolIDs/PrimaryID/IDString"/>;
          <xsl:if                    test ="ProtocolIDs/OtherID/IDString">
          <xsl:value-of              select="ProtocolIDs/OtherID/IDString"/>;
          </xsl:if>
          <xsl:value-of              select="ProtocolTitle
                                             [@Type='Professional']"/>
          <br />
          <br />                       
          </xsl:for-each>
           <br />
           <b><font size='4'>Related Websites
           </font>
           </b>
           <br />
         <xsl:for-each              select ="RelatedWebsites">
          <table width='90%' cellpadding ='0' cellspacing ='0'>
          <tr>
         <th align = 'left' width ='40%'>
          <b><font size='3'>URL
          </font>
          </b>
          </th>
        <td width='60%'>
         <xsl:element name="a">
         <xsl:attribute name="href">
         <xsl:choose>
         <xsl:when                  test   = "contains(RelatedWebsites/
                                              @cdr:xref,'http')">
         <xsl:value-of              select = "RelatedWebsites/
                                              @cdr:xref"/>
         </xsl:when>
         <xsl:otherwise>
          <xsl:value-of              select = "concat('http://',RelatedWebsites/
                                              @cdr:xref)"/>
         </xsl:otherwise>
         </xsl:choose>
        </xsl:attribute>
         <xsl:value-of              select = "RelatedWebsites/
                                              @cdr:xref"/>
        </xsl:element>
        </td>
        </tr>
        <tr>
        <th align='left' width='40%'>
         <b><font size='3'>Name
         </font>
         </b>
         </th>
         <td width='60%'>
        <xsl:value-of              select = "RelatedWebsites"/>
              
        </td>
        </tr>
        </table>
        </xsl:for-each>
        <xsl:for-each              select="Comment">
        <b><font size='3'>Comment
         </font>
         </b>
         <br />
        <xsl:value-of              select="."/>
        <br />
       </xsl:for-each>
    </xsl:template>
    </xsl:transform>]]>
</CdrDocXml>
</CdrDoc>