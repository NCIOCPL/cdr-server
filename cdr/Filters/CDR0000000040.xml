<?xml version="1.0"?>
 <xsl:transform xmlns:xsl="http://www.w3.org/1999/XSL/Transform"
                version="1.0"
                xmlns:cdr="cips.nci.nih.gov/cdr">
  <xsl:output method="html"/>

  <xsl:template match="/">
   <!-- <!DOCTYPE HTML PUBLIC '-//IETF//DTD HTML//EN' /> -->
   <HTML>
    <HEAD> <!-- LINE 10 -->
     <TITLE>@@DOCID@@</TITLE>
     <!-- META HTTP-EQUIV="Pragma" CONTENT="no-cache" / -->
     <LINK REL='stylesheet' 
           HREF='/stylesheets/term.css' 
           TYPE='text/css' />
    </HEAD>
    <BASEFONT FACE='Arial, Helvetica, sans-serif' />
    <xsl:apply-templates select="Org"/>
   </HTML>
  </xsl:template> <!-- LINE 20 -->

  <xsl:template match='Org'>
   <BODY>
    <HR />
    <H1>Organization Document @@DOCID@@</H1>
    <HR />
    <H2><xsl:value-of select="OrgFullName"/></H2>
    <TABLE>
     <xsl:for-each select="OrgCategory">
      <TR>
       <TH ALIGN='right' NOWRAP='1'>Category:</TH>
       <TD><xsl:value-of select="."/></TD>
      </TR>
     </xsl:for-each>
     <xsl:apply-templates select='OrgProductStatus[Product = "Cancer Information Product"]'/>
     <xsl:for-each select="OrgAddressLine">
      <TR>
       <TH ALIGN='right'>Address:</TH>
       <TD><xsl:value-of select="."/></TD>
      </TR>
     </xsl:for-each>
     <TR>
      <TH ALIGN='right'>City:</TH>
      <TD><xsl:value-of select='OrgCity'/></TD>
     </TR>
     <xsl:for-each select="OrgState">
      <TR>
       <TH ALIGN='right' NOWRAP='1'>State:</TH>
       <TD><xsl:value-of select="."/></TD>
      </TR>
     </xsl:for-each>
     <xsl:for-each select="OrgCountry">
      <TR>
       <TH ALIGN='right' NOWRAP='1'>Country:</TH>
       <TD><xsl:value-of select="."/></TD>
      </TR>
     </xsl:for-each>
     <xsl:for-each select="OrgPostalCode">
      <TR>
       <TH ALIGN='right' NOWRAP='1'>Postal Code:</TH>
       <TD><xsl:value-of select="."/></TD>
      </TR>
     </xsl:for-each>
     <xsl:for-each select="OrgPostalCodeLocation">
      <TR>
       <TH ALIGN='right' NOWRAP='1'>Postal Code Location:</TH>
       <TD><xsl:value-of select="."/></TD>
      </TR>
     </xsl:for-each>
     <xsl:for-each select="OrgEmail">
      <TR>
       <TH ALIGN='right' NOWRAP='1'>Email Address:</TH>
       <TD><xsl:value-of select="."/></TD>
      </TR>
     </xsl:for-each>
     <xsl:for-each select="OrgPhone[@Type='voice']">
      <TR>
       <TH ALIGN='right' NOWRAP='1'>Phone:</TH>
       <TD><xsl:value-of select="."/></TD>
      </TR>
     </xsl:for-each>
     <xsl:for-each select="OrgPhone[@Type='facsimile']">
      <TR>
       <TH ALIGN='right' NOWRAP='1'>Fax:</TH>
       <TD><xsl:value-of select="."/></TD>
      </TR>
     </xsl:for-each>
     <xsl:for-each select="OrgIncludeNameFlag">
      <TR>
       <TH ALIGN='right' NOWRAP='1'>Include Name Flag:</TH>
       <TD><xsl:value-of select="."/></TD>
      </TR>
     </xsl:for-each>
     <xsl:for-each select="OrgStopNameFlag">
      <TR>
       <TH ALIGN='right' NOWRAP='1'>Stop Name Flag:</TH>
       <TD><xsl:value-of select="."/></TD>
      </TR>
     </xsl:for-each>
     <xsl:for-each select="OrgSeqNameTopDownFlag">
      <TR>
       <TH ALIGN='right' NOWRAP='1'>Top Down Flag:</TH>
       <TD><xsl:value-of select="."/></TD>
      </TR>
     </xsl:for-each>
     <xsl:for-each select="OrgNoMailerFlag">
      <TR>
       <TH ALIGN='right' NOWRAP='1'>No Mailer Flag:</TH>
       <TD><xsl:value-of select="."/></TD>
      </TR>
     </xsl:for-each>
     <xsl:for-each select="OrgNoMailerFlag">
      <TR>
       <TH ALIGN='right' NOWRAP='1'>Defunct Flag:</TH>
       <TD><xsl:value-of select="."/></TD>
      </TR>
     </xsl:for-each>
     <xsl:for-each select="OrgReviewFlag">
      <TR>
       <TH ALIGN='right' NOWRAP='1' VALIGN='top'>Review Flag:</TH>
       <TD><xsl:value-of select="."/></TD>
      </TR>
     </xsl:for-each>
     <xsl:for-each select="OrgDoesNotTreatFlag">
      <TR>
       <TH ALIGN='right' NOWRAP='1' VALIGN='top'>Does Not Treat Flag:</TH>
       <TD><xsl:value-of select="."/></TD>
      </TR>
     </xsl:for-each>
     <xsl:for-each select="OrgAlternateName[AlternateNameType != 'full name']">
      <TR>
       <TH ALIGN='right' NOWRAP='1' VALIGN='top'>Alternate Name:</TH>
       <TD><xsl:value-of select="AlternateName"/></TD>
      </TR>
     </xsl:for-each>
     <xsl:for-each select="OrgComments">
      <TR>
       <TH ALIGN='right' NOWRAP='1' VALIGN='top'>Comments:</TH>
       <TD><xsl:value-of select="."/></TD>
      </TR>
     </xsl:for-each>
     <xsl:for-each select="PdqKey">
      <TR>
       <TH ALIGN='right' NOWRAP='1'>PDQ Key:</TH>
       <TD><xsl:value-of select="."/></TD>
      </TR>
     </xsl:for-each>
    </TABLE>
   </BODY>
  </xsl:template>

  <xsl:template match='OrgProductStatus'>
     <TR>
      <TH ALIGN='right'>Org Status:</TH>
      <TD><xsl:value-of select='Status'/></TD>
     </TR>
     <TR>
      <TH ALIGN='right'>Status Date:</TH>
      <TD>
       <xsl:value-of select='concat(substring(ChangeDate, 1, 10), " ",
                                    substring(ChangeDate, 12, 8))'/>
      </TD>
     </TR>
  </xsl:template>

 </xsl:transform>