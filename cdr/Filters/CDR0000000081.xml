<?xml version="1.0"?>
 <xsl:transform xmlns:xsl="http://www.w3.org/1999/XSL/Transform"
                version="1.0"
                xmlns:cdr="cips.nci.nih.gov/cdr">
  <xsl:output method="html"/>

  <xsl:template match="/">
   <!-- <!DOCTYPE HTML PUBLIC '-//IETF//DTD HTML//EN' /> -->
   <HTML>
    <HEAD> <!-- LINE 10 -->
     <TITLE>@@DOCID@@</TITLE>
     <!-- META HTTP-EQUIV="Pragma" CONTENT="no-cache" / -->
     <LINK REL='stylesheet' 
           HREF='/stylesheets/term.css' 
           TYPE='text/css' />
    </HEAD>
    <BASEFONT FACE='Arial, Helvetica, sans-serif' />
    <xsl:apply-templates select="InScopeProtocol|OutOfScopeProtocol|ScientificProtocolInfo"/>
   </HTML>
  </xsl:template> <!-- LINE 20 -->

  <xsl:template match='InScopeProtocol|OutOfScopeProtocol|ScientificProtocolInfo'>
   <BODY>
    <HR />
    <H1>Protocol Document @@DOCID@@</H1>
    <HR />
    <H2><xsl:value-of select="ProtocolTitle"/></H2>
    <TABLE>
     <xsl:for-each select="ProtocolTitle">
      <TR>
       <TH ALIGN='right' NOWRAP='1'>Protocol Title:</TH>
       <TD><xsl:value-of select="."/></TD>
      </TR>
     </xsl:for-each>
     <xsl:for-each select="IdentificationInfo/ProtocolPDQID">
      <TR>
       <TH ALIGN='right' NOWRAP='1'>PDQ ID:</TH>
       <TD><xsl:value-of select="."/></TD>
      </TR>
     </xsl:for-each>
     <xsl:for-each select="IdentificationInfo/ProtocolIDs/PrimaryID/IDstring">
      <TR>
       <TH ALIGN='right' NOWRAP='1'>Primary ID:</TH>
       <TD><xsl:value-of select="."/></TD>
      </TR>
     </xsl:for-each>
     <xsl:for-each select="IdentificationInfo/ProtocolIDs/OtherID/IDstring">
      <TR>
       <TH ALIGN='right' NOWRAP='1'>Other ID:</TH>
       <TD><xsl:value-of select="."/></TD>
      </TR>
     </xsl:for-each>
     <xsl:for-each select="ProtocolSources/ProtocolSource/SourceName">
      <TR>
       <TH ALIGN='right' NOWRAP='1'>Protocol Source:</TH>
       <TD><xsl:value-of select="."/></TD>
      </TR>
     </xsl:for-each>
     <xsl:for-each select="ProtocolApproval/ReviewApprovalType">
      <TR>
       <TH ALIGN='right' NOWRAP='1'>Approval Type:</TH>
       <TD><xsl:value-of select="."/></TD>
      </TR>
     </xsl:for-each>
     <xsl:for-each select="ProtocolAdminInfo/ProtocolLeadOrg/LeadOrganizationID">
      <TR>
       <TH ALIGN='right' NOWRAP='1'>Lead Organization:</TH>
       <TD><xsl:value-of select="."/></TD>
      </TR>
     </xsl:for-each>
    </TABLE>
   </BODY>
  </xsl:template>

 </xsl:transform>