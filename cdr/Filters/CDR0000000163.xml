<CdrDoc Type='Filter' Id='CDR0000000163'>
<CdrDocCtl>
<DocValStatus readonly="yes">U</DocValStatus>
<DocActiveStatus readonly="yes">A</DocActiveStatus>
<DocTitle readonly="yes">Glossary Term Display for Patient Summaries</DocTitle>
<ReadyForReview readonly="yes">N</ReadyForReview>
</CdrDocCtl>
<CdrDocXml><![CDATA[<?xml version="1.0"?>
  <xsl:transform xmlns:xsl="http://www.w3.org/1999/XSL/Transform"
                 version="1.0">
   <xsl:output method="html"/>
  <!-- Written by Nanci, 5/14/2002 -->

   <!--=============================================================
         Displays Glossary Term, Pronunciation, and Definition
       =============================================================-->

   <xsl:template match="GlossaryTerm">
      <xsl:value-of select="TermName"/>
     <xsl:if test="TermPronunciation">
          <xsl:text>&#xa0;&#xa0;</xsl:text>
          <xsl:value-of select="TermPronunciation"/>
      </xsl:if>: <br /><br />
      <xsl:value-of select="TermDefinition/DefinitionText"/>
   </xsl:template>

  </xsl:transform>]]>
</CdrDocXml>
</CdrDoc>