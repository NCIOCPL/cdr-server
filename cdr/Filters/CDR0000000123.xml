<CdrDoc Type='Filter' Id='CDR0000000123'>
<CdrDocCtl>
<DocValStatus readonly="yes">U</DocValStatus>
<DocActiveStatus readonly="yes">A</DocActiveStatus>
<DocTitle readonly="yes">InScope Protocol Administrative Report Filter</DocTitle>
</CdrDocCtl>
<CdrDocXml><![CDATA[<?xml version="1.0"?>
<!--Filter written by C.Burg - 3/13/2002 -->

<!-- ================================================================
           Filter to generate a Protocol Administrative QC Report
     ================================================================ -->
  
  <xsl:transform       xmlns:xsl="http://www.w3.org/1999/XSL/Transform"
                         version="1.0"
                       xmlns:cdr="cips.nci.nih.gov/cdr">
   <xsl:output method="html"/>

<!-- ===============================================================
                    Generate Report Headings
     =============================================================== -->

   <xsl:template match="/">
  
 
   <!-- <!DOCTYPE HTML PUBLIC '-//IETF//DTD HTML//EN' /> -->
    <HTML>
    <HEAD>
    <xsl:variable name="cdrmydoc" select="document('cdrx:/*/CdrCtl')"/>

    <TITLE><xsl:value-of select="document('cdrutil:/docid')"/>&#xa0;&#xa0;<xsl:value-of select="substring-before($cdrmydoc/CdrDocCtl/DocTitle,';')"/></TITLE>
     <!--
     <LINK REL='stylesheet' 
           HREF='/stylesheets/protocol2.css' 
           TYPE='text/css' /> -->
    </HEAD>
    <BASEFONT FACE='Arial, Helvetica, sans-serif' /> 
    <xsl:apply-templates select="InScopeProtocol"/>
    </HTML>
    </xsl:template>
    <xsl:template match="InScopeProtocol">
    <BODY>
    <b><font size='4'><center>Administrative Protocol Information<br />QC Report</center></font></b>
 
 <b><font size='3'><center><xsl:value-of select="document('cdrutil:/date/%25B %25#d, %25Y %25#I:%25M%25p')"/></center></font></b><br />
<b><xsl:value-of select="document('cdrutil:/docid')"/></b><br />

<!-- ==================================================================
                       Display the Professional Title
     ================================================================== -->
 
  <b><font size='4'>Professional Title</font></b><br />
  <xsl:apply-templates  select="ProtocolTitle[@Type='Professional']"/><br /><br />

<!-- ==============================================================
               Display the Identification Information
     =============================================================== -->

  <b><font size='4'>Identification Information</font></b><br /><br />

<b><font size='3'>PDQ ID</font></b><xsl:text>...................................................</xsl:text> 
<xsl:apply-templates select="PdqKey"/><br />


<b><font size='3'>Primary ID</font></b><xsl:text>.............................................</xsl:text>
<xsl:apply-templates select="ProtocolIDs/PrimaryID/IDString"/><br />

<b><font size='3'>Comments</font></b><br />
<xsl:apply-templates select="ProtocolIDs/PrimaryID/Comment"/><br /><br />

<xsl:for-each select="ProtocolIDs/OtherID">
<b><font size='3'>Other ID</font></b><xsl:text>..................................................</xsl:text>
<xsl:value-of select="IDType"/>,
<xsl:value-of select="IDString"/><br />
<b><font size='3'>Comments</font></b><br />
<xsl:apply-templates select="ProtocolIDs/OtherID/Comment"/><br />
</xsl:for-each>
<br /><br />

<!-- ============================================================
              Display the funding information
     ============================================================ -->
 
<b><font size='4'>Funding Information</font></b><br /><br />
<xsl:for-each select="FundingInfo/NIHGrantContract">
<b><font size='3'>NIH Grant Contract Type</font></b><xsl:text>.......................</xsl:text>

<xsl:value-of select="NIHGrantContractType"/><br />
<b><font size='3'>Grant Contract Number</font></b> <xsl:text>.........................</xsl:text>
<xsl:value-of select="GrantContractNo"/><br />
<b><font size='3'>Comments</font></b><br />
<xsl:apply-templates select="Comment"/>
</xsl:for-each>
<br /><br />

<!-- ================================================================
                   Display the Protocol Source
     ================================================================= -->


<b><font size='4'>Protocol Source</font></b><br /><br />
<xsl:for-each select="ProtocolSources/ProtocolSource">
<b><font size='3'>Source </font></b><xsl:text>.....................................................</xsl:text>
<xsl:value-of select="SourceName"/><br />
<b><font size='3'>Date Received</font></b><xsl:text>........................................</xsl:text>
<xsl:value-of select="DateReceived"/><br />
</xsl:for-each>
<br />

<!-- ====================================================================
                Display the Protocol Approval Information
     ===================================================================== -->

<b><font size='4'>Protocol Approval</font></b><br /><br />
<xsl:for-each select="ProtocolApproval">
<b><font size='3'>Review Approval Type</font></b><xsl:text>..........................</xsl:text>
<xsl:value-of select="ReviewApprovalType"/><br />
<b><font size='3'>Reimbursement Approval Type</font></b> <xsl:text>...........</xsl:text>
<xsl:value-of select="ReimbursementApprovalType"/><br />
<b><font size='3'>Comments</font></b><br />
<xsl:apply-templates select="Comment"/>
</xsl:for-each>
<br /><br />

<!-- ===============================================================
                  Display the Protocol Sponsors
     ================================================================ -->

<b><font size='4'>Protocol Sponsors</font></b><br /><br />
<xsl:for-each select="ProtocolSponsors/SponsorName">
<b><font size='3'>Name</font></b> <xsl:text>......................................................</xsl:text>
<xsl:value-of select="."/><br />
<b><font size='3'>Comments</font></b><br /><br />
<xsl:value-of select="Comment"/>
</xsl:for-each>
<br />

<!-- ====================================================================
                  Display the Protocol Status Information
     ==================================================================== -->

<b><font size='4'>Current Protocol Status</font></b><br /><br />
<xsl:for-each select="ProtocolAdminInfo/CurrentProtocolStatus">
<b><font size='3'>Status</font></b> <xsl:text>.....................................................</xsl:text>
<xsl:value-of select="."/><br />
</xsl:for-each>
<br />

<!-- ====================================================================
              Display the Lead Organization Information
     ==================================================================== -->

<xsl:for-each select="ProtocolAdminInfo">
<xsl:for-each select="ProtocolLeadOrg">
<b><font size='4'>Lead Organization</font></b><br /><br />
<b><font size='3'>Organization Name</font></b> <xsl:text>.............................</xsl:text>
<xsl:value-of select="OfficialName/Name"/><br />
<b><font size='3'>Organization Role</font></b> <xsl:text>...............................</xsl:text>
<xsl:value-of select="LeadOrgRole"/><br />
<b><font size='3'>Protocol ID</font></b> <xsl:text>..........................................</xsl:text>
<xsl:value-of select="LeadOrgProtocolID"/><br />
<b><font size='3'>Current Protocol Status</font></b> <br />
<xsl:text>&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;</xsl:text>
<b><font size='3'>Status</font></b> 
<xsl:text>....................................................</xsl:text>
<xsl:value-of select="LeadOrgProtocolStatuses/CurrentOrgStatus/StatusName"/><br />
<xsl:text>&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;</xsl:text>
 <b><font size='3'>Date</font></b>
 <xsl:text>........................................................</xsl:text>
<xsl:value-of select="LeadOrgProtocolStatuses/CurrentOrgStatus/StatusDate"/><br />
<xsl:text>&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;</xsl:text>
 <b><font size='3'>User</font></b>
 <xsl:text>........................................................</xsl:text>
<xsl:value-of select="LeadOrgProtocolStatuses/CurrentOrgStatus/EnteredBy"/><br />
<xsl:text>&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;</xsl:text>
 <b><font size='3'>Entry Date</font></b>
 <xsl:text>..............................................</xsl:text>
<xsl:value-of select="LeadOrgProtocolStatuses/CurrentOrgStatus/EntryDate"/><br />

<b><font size='3'>Previous Protocol Status</font></b><br />
<xsl:text>&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;</xsl:text>
 <b><font size='3'>Status</font></b>
 <xsl:text>.....................................................</xsl:text>
<xsl:value-of select="LeadOrgProtocolStatuses/PreviousOrgStatus/StatusName"/><br />
<xsl:text>&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;</xsl:text>
 <b><font size='3'>Date</font></b>
 <xsl:text>........................................................</xsl:text>
<xsl:value-of select="LeadOrgProtocolStatuses/PreviousOrgStatus/StatusDate"/><br />
<xsl:text>&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;</xsl:text>
 <b><font size='3'>User</font></b>
 <xsl:text>........................................................</xsl:text>
<xsl:value-of select="LeadOrgProtocolStatuses/PreviousOrgStatus/EnteredBy"/><br />
<xsl:text>&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;</xsl:text>
 <b><font size='3'>Entry Date</font></b>
 <xsl:text>..............................................</xsl:text>
<xsl:value-of select="LeadOrgProtocolStatuses/PreviousOrgStatus/EntryDate"/><br />
<br />
<!--</xsl:for-each>-->

<!-- ==================================================================
               Display Lead Organization Personnel Information
     ================================================================= -->

<b><font size='3'>Lead Organization Personnel</font></b><br /><br />

<xsl:for-each select="LeadOrgPersonnel">


<xsl:for-each select="Person">
<b><font size='3'>Person</font></b>
 <xsl:text>.............................................</xsl:text>
<xsl:value-of select="PersonNameInformation/SurName"/>,
<xsl:value-of select="PersonNameInformation/GivenName"/><br />
<b><font size='3'>Role</font></b> <xsl:text>..................................................</xsl:text>
<xsl:for-each select="../PersonRole">
<xsl:value-of select="."/><br /><xsl:text>&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;
&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;
&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;
&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;
&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;
&#xa0;</xsl:text>
</xsl:for-each>
<br />

<b><font size='3'>Organization Name</font></b>
<xsl:text>......................</xsl:text>
<xsl:value-of select="OrganizationLocation"/><br />
          
</xsl:for-each>
 <b><font size='3'>Contact</font></b><br />
<xsl:text>&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;</xsl:text>
 <b><font size='3'>Street</font></b>
<xsl:text>.......................................</xsl:text>
<xsl:for-each select="Person/Street">
<xsl:value-of select="."/><br />
&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;
&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;
&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;
&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;
&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;
 </xsl:for-each>
<br />
<xsl:for-each select="Person/City">
<xsl:text>&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;</xsl:text>
 <b><font size='3'>City</font></b>
<xsl:text>.........................................</xsl:text>
<xsl:value-of select="."/><br />
<xsl:text>&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;</xsl:text>
<b><font size='3'>State/Political Unit</font></b>
<xsl:text>..............</xsl:text>
<xsl:value-of select="../PoliticalSubUnit_State/PoliticalSubUnitFullName"/><br />
<xsl:text>&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;</xsl:text>
<b><font size='3'>Country</font></b>
<xsl:text>.................................</xsl:text>
<xsl:value-of select="../Country/CountryFullName"/><br />
<xsl:text>&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;</xsl:text>
<b><font size='3'>Zip/Postal Code</font></b>
<xsl:text>..................</xsl:text>
<xsl:value-of select="../PostalCode_ZIP"/><br />
<xsl:text>&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;</xsl:text>
<b><font size='3'>Phone</font></b>
<xsl:text>....................................</xsl:text>
<xsl:value-of select="../Phone"/>&#xa0;
<xsl:if test="../Phone/@Public">
<xsl:text>(Public=</xsl:text>
<xsl:value-of select="../Phone/@Public"/>
<xsl:text>)</xsl:text>
</xsl:if>
<br />
<xsl:text>&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;</xsl:text>
<b><font size='3'>Fax</font></b>
<xsl:text>.........................................</xsl:text>
<xsl:value-of select="../Fax"/><br />
<xsl:text>&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;</xsl:text>
<b><font size='3'>Email</font></b>
<xsl:text>........................................</xsl:text>
<xsl:value-of select="../Email"/>&#xa0;
<xsl:if test="../Email/@Public">
<xsl:text>(Public=</xsl:text>
<xsl:value-of select="../Email/@Public"/>
<xsl:text>)</xsl:text>
</xsl:if>
<br />

</xsl:for-each>
<br />
</xsl:for-each>

<!-- ================================================================
      Display Participating Orgs Name, Status, and Contact Information
     ================================================================= -->

<b><font size='3'>Participating Organizations (for Lead Organization) </font></b><br /><br />
      <xsl:for-each select="ProtocolSites">
      <xsl:for-each select="OrgSite">  
      <xsl:value-of select="position()"/>.      
      <b><font size='3'>OrganizationName</font></b>
        <xsl:text>................................</xsl:text>
        <xsl:value-of select="OfficialName/Name"/><br />
       

        <b><font size='3'>&#xa0;&#xa0;&#xa0;&#xa0;Organization Protocol Status</font></b>
        <xsl:text>.............</xsl:text>
        <xsl:value-of select="OrgSiteStatus"/><br />

        <xsl:for-each select="OrgSiteContact">
         
       <xsl:if test="SpecificPerson">
        
    <b><font size='3'>&#xa0;&#xa0;&#xa0;&#xa0;Contact</font></b>
          <xsl:text>.....................................................</xsl:text>
          <xsl:value-of select="SpecificPerson/PersonNameInformation/SurName"/>,
          <xsl:value-of select="SpecificPerson/PersonNameInformation/GivenName"/><br />

        <b><font size='3'>&#xa0;&#xa0;&#xa0;&#xa0;Role</font></b>
                <xsl:text>............................................................</xsl:text>
          <xsl:value-of select="SpecificPerson/Role"/><br />

 
          <b><font size='3'>&#xa0;&#xa0;&#xa0;&#xa0;Phone</font></b>
                <xsl:text>........................................................</xsl:text>
          <xsl:value-of select="SpecificPerson/Phone"/>&#xa0;
               <xsl:if test="SpecificPerson/Phone/@Public">
               <xsl:text>(Public=</xsl:text>
               <xsl:value-of select="SpecificPerson/Phone/@Public"/>
               <xsl:text>)</xsl:text>
               </xsl:if>
               <br />
          
 <b><font size='3'>&#xa0;&#xa0;&#xa0;&#xa0;Toll Free Phone</font></b>
                <xsl:text>.......................................</xsl:text>
          <xsl:value-of select="SpecificPerson/TollFreePhone"/><br />
         <b><font size='3'>&#xa0;&#xa0;&#xa0;&#xa0;Fax</font></b>
                <xsl:text>..............................................................</xsl:text>
          <xsl:value-of select="SpecificPerson/Fax"/><br />
         <b><font size='3'>&#xa0;&#xa0;&#xa0;&#xa0;Email</font></b>
         <xsl:text>..........................................................</xsl:text>
         
          <xsl:value-of select="SpecificPerson/Email"/>&#xa0;
          <xsl:if test="SpecificPerson/Email/@Public">
          <xsl:text>(Public=</xsl:text>
          <xsl:value-of select="SpecificPerson/Email/@Public"/>
          <xsl:text>)</xsl:text>
          </xsl:if>
          <br />
  
          <b><font size='3'>&#xa0;&#xa0;&#xa0;&#xa0;URL</font></b>
         <xsl:text>.............................................................</xsl:text>
          <xsl:value-of select="SpecificPerson/WebSite"/><br /><br />     
                
          </xsl:if>  
         <xsl:if test="GenericPerson">
        <b><font size='3'>Generic Title</font></b>
          <xsl:text>.............................................</xsl:text>
          <xsl:value-of select="GenericPerson/PersonTitle"/><br />

          <b><font size='3'>Generic Contact</font></b>
          <xsl:text>.....................................</xsl:text>
          <xsl:value-of select="GenericPerson/ContactName"/><br />
          <b><font size='3'>Address</font></b>
                <xsl:text>....................................................</xsl:text>
          <xsl:for-each select="GenericPerson/Street">
          <xsl:value-of select="."/>,
          </xsl:for-each>
          <xsl:value-of select="GenericPerson/City"/>,
          <xsl:value-of select="GenericPerson/PoliticalSubUnit_State"/>
          <xsl:text>&#xa0;</xsl:text>
         <xsl:value-of select="GenericPerson/PostalCode_ZIP"/><br />
                                                                                                               
          <b><font size='3'>Phone</font></b>
                <xsl:text>........................................................</xsl:text>
          <xsl:value-of select="GenericPerson/Phone"/>&#xa0;
          <xsl:if test="GenericPerson/Phone/@Public">
          <xsl:text>(Public=</xsl:text>
          <xsl:value-of select="GenericPerson/Phone/@Public"/>
          <xsl:text>)</xsl:text>
          </xsl:if>
          <br />
        
         <b><font size='3'>Toll Free Phone</font></b>
                <xsl:text>.......................................</xsl:text>
          <xsl:value-of select="GenericPerson/TollFreePhone"/><br />
         <b><font size='3'>Fax</font></b>
                <xsl:text>..............................................................</xsl:text>
          <xsl:value-of select="GenericPerson/Fax"/><br />
         <b><font size='3'>Email</font></b>
         <xsl:text>..........................................................</xsl:text>
         
          <xsl:value-of select="GenericPerson/Email"/>&#xa0;  
          <xsl:if test="GenericPerson/Email/@Public">
          <xsl:text>(Public=</xsl:text>
          <xsl:value-of select="GenericPerson/Email/@Public"/>
          <xsl:text>)</xsl:text>
          </xsl:if>
          <br />
  
          <b><font size='3'>URL</font></b>
         <xsl:text>.............................................................</xsl:text>
          <xsl:value-of select="GenericPerson/WebSite"/><br />               
          </xsl:if>       
          </xsl:for-each>
          </xsl:for-each>
          </xsl:for-each>
          </xsl:for-each>
          </xsl:for-each>
  </BODY>
  </xsl:template>
  </xsl:transform>]]>
</CdrDocXml>
