<CdrDocXml><![CDATA[<?xml version="1.0"?>
<!--Filter written by C.Burg - 3/13/2002 -->

<!-- ================================================================
           Filter to generate a Protocol Administrative QC Report
     ================================================================ -->
  
  <xsl:transform       xmlns:xsl="http://www.w3.org/1999/XSL/Transform"
                         version="1.0"
                       xmlns:cdr="cips.nci.nih.gov/cdr">
   <xsl:output method="html"/>

<!-- ===============================================================
                    Generate Report Headings
     =============================================================== -->

   <xsl:template match="/">
  
 
   <!-- <!DOCTYPE HTML PUBLIC '-//IETF//DTD HTML//EN' /> -->
    <HTML>
    <HEAD>
    <xsl:variable name="cdrmydoc" select="document('cdrx:/*/CdrCtl')"/>

    <TITLE><xsl:value-of select="document('cdrutil:/docid')"/>&#xa0;&#xa0;<xsl:value-of select="substring-before($cdrmydoc/CdrDocCtl/DocTitle,';')"/></TITLE>
     <!--
     <LINK REL='stylesheet' 
           HREF='/stylesheets/protocol2.css' 
           TYPE='text/css' /> -->
    </HEAD>
    <BASEFONT FACE='Arial, Helvetica, sans-serif' /> 
    <xsl:apply-templates select="InScopeProtocol"/>
    </HTML>
    </xsl:template>
    <xsl:template match="InScopeProtocol">
    <BODY>
    <b><font size='4'><center>Administrative Protocol Information<br />QC Report</center></font></b>
 
 <b><font size='3'><center><xsl:value-of select="document('cdrutil:/date/%25B %25#d, %25Y %25#I:%25M%25p')"/></center></font></b><br />
<b><xsl:value-of select="document('cdrutil:/docid')"/></b><br />

<!-- ==================================================================
              Display the Professional and Original Title
     ================================================================== -->
 
  <b><font size='4'>Professional Title</font></b><br />
  <xsl:apply-templates  select="ProtocolTitle[@Type='Professional']"/><br /><br />
  
    <b><font size='4'>Original Title</font></b><br />
  <xsl:apply-templates  select="ProtocolTitle[@Type='Original']"/><br /><br />


<!-- ==============================================================
               Display the Identification Information
     =============================================================== -->

  <b><font size='4'>Identification Information</font></b><br /><br />
 <table width='90%'>
  <tr>
  <th align='left' width='40%'>
<b><font size='3'>PDQ ID
   </font>
 </b>
</th>
 <td width='60%'>
 <xsl:apply-templates               select="PdqKey"/>
 </td>
 </tr>
 
  <tr>
  <th align='left' width='40%'>
  <b><font size='3'>Primary ID
     </font>
  </b>
  </th>
  <td width='60%'>
 <xsl:apply-templates select="ProtocolIDs/PrimaryID/IDString"/><br />
 </td>
 </tr>
 </table>
<b><font size='3'>Comments
   </font>
   </b>
   <br />

<xsl:apply-templates          select="ProtocolIDs/PrimaryID/Comment"/>
    <br /><br />
 
 <xsl:for-each                select="ProtocolIDs/OtherID">
   <table width='90%'>
   <tr align='left' width='40%'>
   <th>
  <b><font size='3'>Other ID
   </font>
  </b>
  </th>
   </tr>
   </table>

   <table width='90%'>
    <tr>
   <th align='left' width='40%'>
  <b><font size='3'>ID Type
  </font>
  </b>
  </th>
  <td width='60%'>
  <xsl:value-of                 select="IDType"/>,
  <xsl:value-of select="IDString"/>
  </td>
  </tr>
  </table>
  <b><font size='3'>Comments
   </font>
  </b>
 <br />
<xsl:apply-templates          select="Comment"/>
 <br />
</xsl:for-each>
<br /><br />

   <!-- =============================================================
               Display Protocol Special Category
       ============================================================ -->

           
              <xsl:if test="not(preceding-sibling::ProtocolSpecialCategory)">
              <b>
              <font size='4'>
              <xsl:text>Protocol Special Category</xsl:text>
              </font>
              </b>
              <br /><br />
              </xsl:if>
               <table width='90%'> 
              <tr>
              <th align='left' width='40%'>   
              <b><font size='3'>Special Category
              </font></b>
              </th>
              <td width='60%'>
              <xsl:value-of      select="ProtocolSpecialCategory/
                                         SpecialCategory"/>              
              </td>
              </tr>
              </table>
              <b><font size='3'>Comment
              </font></b><br />
              <xsl:value-of          select="ProtocolSpecialCategory/
                                             Comment"/>
               <br />
               <br />
          
                           
              
              


<!-- ============================================================
              Display the funding information
     ============================================================ -->
 
      <b><font size='4'>Funding Information</font></b>
       <br /><br />
      <xsl:for-each            select="FundingInfo/NIHGrantContract">
      <table width='90%'>
      <tr>
      <th align='left' width='40%'>
      <b><font size='3'>NIH Grant Contract Type
         </font>
       </b>
       </th>
       <td>
     <xsl:value-of             select="NIHGrantContractType"/>
       </td>
       </tr>
       <tr>
      <th align='left' width='40%'>
      <b><font size='3'>Grant Contract Number
         </font>
      </b>
      </th>
      <td width='60%'>
     <xsl:value-of            select="GrantContractNo"/><br />
      </td>
      </tr>
      </table>
       <b><font size='3'>Comment
         </font>
      </b>
      <br />
      <xsl:apply-templates select="Comment"/>
      </xsl:for-each>
   
       <br />
       <br />
       <b><font size='3'>Other Grant Contract Information</font></b>
     <xsl:for-each   select="FundingInfo/OtherGrantContractInfo"> 
     <table width='90%'>
      <tr>
      <th align='left' width='40%'>
      <b>
      <font size='3'>Grant Contract Number
      </font>
      </b> 
     </th>

     <td width='60%'>
     <xsl:value-of                select="GrantContractNo"/>
                                          
     </td>
     </tr>
     </table>
    <b><font size='3'>Comment</font></b><br />
     <xsl:value-of                select="Comment"/>
    <br />                                      
    </xsl:for-each>
    <br /><br />

   

<!-- ================================================================
                   Display the Protocol Source
     ================================================================= -->


           <b><font size='4'>Protocol Source</font></b>
                   <br /><br />
          <xsl:for-each               select="ProtocolSources/ProtocolSource">
            <table width='90%'>
            <tr>
           <th align='left' width='40%'>
           <b><font size='3'>Source
           </font>
          </b>
          </th>
          <td width='60%'>
          <xsl:value-of                  select="SourceName"/><br />
          </td>
          </tr>
           <tr>
           <th align='left' width='40%'>
           <b><font size='3'>Date Received
           </font>
          </b>
          </th>
          <td width='60%'>
          <xsl:value-of                select="DateReceived"/>
          </td>
          </tr>
         </table>
         </xsl:for-each>
           <br />

<!-- ====================================================================
                Display the Protocol Approval Information
     ===================================================================== -->

       <b><font size='4'>Protocol Approval</font></b>
                       <br /><br />
       <xsl:for-each              select="ProtocolApproval">
           <table width='90%'>
           <tr>
           <th align='left' width='40%'>
           <b><font size='3'>Review ApprovalType
           </font>
          </b>
          </th>
          <td width='40%'>
        <xsl:value-of            select="ReviewApprovalType"/><br />
        </td>
        </tr>
          <tr>
           <th align='left' width='40%'>
           <b><font size='3'>Reimbursement Approval Type
           </font>
          </b>
          </th>
          <td width='60%'>
         <xsl:value-of            select="ReimbursementApprovalType"/>
         </td>
         </tr>
         </table>
  <b><font size='3'>Comments
     </font>
  </b>
   <br />
   <xsl:apply-templates select="Comment"/>
   </xsl:for-each>
         <br /><br />

<!-- ===============================================================
                  Display the Protocol Sponsors
     ================================================================ -->

     <b><font size='4'>Protocol Sponsors</font></b>
     <br /><br />
      <xsl:for-each                 select="ProtocolSponsors/SponsorName">
           <table width='90%'>
           <tr>
           <th align='left' width='40%'>
           <b><font size='3'>Name
           </font>
          </b>
          </th>
          <td width='60%'>
         <xsl:value-of             select="."/>
         </td>
         </tr>
         </table>         
         <b><font size='3'>Comments</font></b>
              <br />
        <xsl:value-of              select="../Comment"/>
        </xsl:for-each>
            <br />

<!-- ====================================================================
                  Display the Protocol Status Information
     ==================================================================== -->

      <b><font size='4'>Current Protocol Status</font></b>
      <br /><br />
      <xsl:for-each            select="ProtocolAdminInfo/
                                       CurrentProtocolStatus">
           <table width='90%'>
           <tr>
           <th align='left' width='40%'>
           <b><font size='3'>Status
           </font>
          </b>
          </th>
          <td width='60%'>
          <xsl:value-of                select="."/>
          </td>
          </tr>
          </table>
          <br />
          </xsl:for-each>
          <br />

<!-- ====================================================================
              Display the Lead Organization Information
     ==================================================================== -->

    <xsl:for-each                      select="ProtocolAdminInfo">
    <xsl:for-each                      select="ProtocolLeadOrg">
    <b>
   <font size='4'>Lead Organization
   </font>
   </b>
      <br /><br />
       <table width='90%'>
        <tr>
       <th align='left' width='40%'>
       <b><font size='3'>Organization Name</font></b>
       </th>
       <td width='60%'>
       <xsl:value-of                    select="LeadOrganizationID/
                                                Organization/
                                                OrganizationNameInformation/
                                                OfficialName/Name"/>
        </td>
        </tr>
        </table>
        <table width='90%'>
        <tr>
       <th align='left' width='40%'>
       <b><font size='3'>Organization Role</font></b>
       </th>
     <td width='60%'>
      <xsl:value-of             select="LeadOrgRole"/>
      </td>
      </tr>
      </table>
      <table width='90%'>
       <tr>
      <th align='left' width='40%'>
       <b><font size='3'>Protocol ID</font></b>
       </th>
      <td width='60%'>      
       <xsl:value-of               select="LeadOrgProtocolID"/>
       </td>
       </tr>
       </table>
       <table width='90%'>
       <tr>
       <th align='left' width='40%'>
       <b><font size='3'>Current Protocol Status</font></b>
       </th>
       <td>
       <xsl:text>&#xa0;</xsl:text>
       </td>
       </tr>
       </table>
       <table width='90%'>
       <tr>
       <th align='left' width='40%'>
       <xsl:text>&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;
                 &#xa0;&#xa0;&#xa0;&#xa0;</xsl:text>
       <b><font size='3'>Status
       </font>
       </b>
       </th>
       <td width='60%'>
       <xsl:value-of                     select="LeadOrgProtocolStatuses/
                                        CurrentOrgStatus/StatusName"/>
       </td>
        </tr>
       </table>
        <table width='90%'>     
        <tr>
       <th align='left' width='40%'>
        <xsl:text>&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;
                  &#xa0;&#xa0;&#xa0;&#xa0;</xsl:text>
        <b><font size='3'>Date</font></b>
       </th>
       <td width='60%'>
       <xsl:value-of                  select="LeadOrgProtocolStatuses/
                                              CurrentOrgStatus/StatusDate"/>
       </td>
       </tr>
       </table>
       <table width='90%'>
       <tr>
       <th align='left' width='40%'>
        <xsl:text>&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;
                 &#xa0;&#xa0;&#xa0;&#xa0;</xsl:text>
        <b><font size='3'>User</font></b>
       </th>
       
      <td width='60%'>
      <xsl:value-of               select="LeadOrgProtocolStatuses/
                                          CurrentOrgStatus/EnteredBy"/>
       </td>
       </tr>
       </table>
       <table width='90%'>
       <tr>
       <th align='left' width='40%'>
        <xsl:text>&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;
                 &#xa0;&#xa0;&#xa0;&#xa0;</xsl:text>
        <b><font size='3'>Entry Date</font></b>
       </th>
      <td width='60%'>
      <xsl:value-of               select="LeadOrgProtocolStatuses/
                                          CurrentOrgStatus/EntryDate"/>
       </td>
       </tr>
       </table>
       <table width='90%'>
       <tr>
       <th align='left' width='40%'>
       <b><font size='3'>Previous Protocol Status</font></b>
       </th>
       <td>
       <xsl:text>&#xa0;</xsl:text>
       </td>
       </tr>
        </table>
       <xsl:if                    test="PreviousProtocolStatus">
       <table width='90%'>
       <tr>
       <th align='left' width='40%'>
       <xsl:text>&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;
                 &#xa0;&#xa0;&#xa0;&#xa0;</xsl:text>
       <b><font size='3'>Status
       </font>
       </b>
       </th>
      <td width='60%'>
       <xsl:value-of                     select="LeadOrgProtocolStatuses/
                                                PreviousOrgStatus/
                                                StatusName"/>
        </td>
        </tr>
       </table>
       <table width='90%'>
       <tr>
       <th align='left' width='40%'>
        <xsl:text>&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;
                  &#xa0;&#xa0;&#xa0;&#xa0;</xsl:text>
        <b><font size='3'>Date</font></b>
       </th>
       <td width='60%'>
       <xsl:value-of               select="LeadOrgProtocolStatuses/
                                           PreviousOrgStatus/StatusDate"/>
       </td>
       </tr>
       </table>
       <table width='90%'>
       <tr>
       <th align='left' width='40%'>
        <xsl:text>&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;
                 &#xa0;&#xa0;&#xa0;&#xa0;</xsl:text>
        <b><font size='3'>User</font></b>
       </th>
       <td width='60%'>
      <xsl:value-of               select="LeadOrgProtocolStatuses/
                                          PreviousOrgStatus/EnteredBy"/>
       </td>
       </tr>
       </table>
       <table width='90%'>
       <tr>
       <th align='left' width='40%'>
        <xsl:text>&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;
                 &#xa0;&#xa0;&#xa0;&#xa0;</xsl:text>
        <b><font size='3'>Entry Date</font></b>
       </th>
       
      <td width='60%'>
      <xsl:value-of               select="LeadOrgProtocolStatuses/
                                          PreviousOrgStatus/EntryDate"/>
       </td>
       </tr>
       </table>
      </xsl:if>

<!-- ==================================================================
               Display Lead Organization Information
     ================================================================= -->

     <br />
     <b><font size='3'>Lead Organization Personnel
        </font>
     </b>
    <br /><br />
    <xsl:for-each                          select="LeadOrgPersonnel/Person">
         <xsl:variable name="orgref"       select="Person/@cdr:ref"/>
         <xsl:variable name="OrgFrag"      select="substring-after 
                                                  ($orgref,'#')"/>
        
        <table width='90%'>
         <tr>
         <th align='left' width='40%'>
        <b><font size='3'>Person
         </font>
        </b>
        </th>
        <td width='60%'>
         <xsl:value-of             select="Person/
                                           PersonNameInformation/SurName"/>,
          <xsl:value-of            select="Person/PersonNameInformation/
                                           GivenName"/>&#xa0;
         <xsl:variable name="personref"
                                   select="Person/@cdr:ref"/>
         <xsl:variable name="FragPersonRef" 
                                   select="substring-after($personref,'#')"/>
        <xsl:text>[</xsl:text>
        <xsl:value-of              select="$FragPersonRef"/>
         <xsl:text>]</xsl:text>
         </td>
         </tr>
         </table>
         <xsl:for-each             select="../PersonRole">
         <table width='90%'>
         <tr>
         <th align='left' width='40%'>
        <xsl:text>Role</xsl:text>
       </th>
       <td width='60%'>            
      <xsl:value-of               select="."/>
     </td>
     </tr>
     </table>
     </xsl:for-each>
     <table width='90%'>
      <tr>
      <th align='left' width='40%'>
      <xsl:text>Organization Name</xsl:text>
      </th>
           
     <td>
    <xsl:value-of                    select="Person/PersonLocations/
                                             OtherPracticeLocation
                                             [@cdr:id=$OrgFrag]/
                                             OrganizationLocation/
                                             Organization/
                                             OrganizationNameInformation/
                                             OfficialName/Name"/>
     </td>
     </tr>
     </table>
     <table width='90%'>
     <tr>
     <th align='left' width='40%'>
      <xsl:text>Contact</xsl:text>
      </th>
      </tr>
     </table>
   
     
       <xsl:if                test="Person/PersonLocations/
                                    OtherPracticeLocation 
                                    [@cdr:id=$OrgFrag]/
                                    SpecificPostalAddress"> 
    
      <table width='90%'>  
      <tr>
      <th align='left' width='40%'>
      <xsl:text>&#xa0;&#xa0;&#xa0;Street</xsl:text>
      </th>
      </tr>
      </table>

       <xsl:for-each           select="Person/PersonLocations/
                                       OtherPracticeLocation  
                                       [@cdr:id=$OrgFrag]/
                                       SpecificPostalAddress/Street">
      <table width='90%'>  
      <tr>
      <th align='left' width='40%'>
      <xsl:text>&#xa0;&#xa0;</xsl:text>
      </th>
      <td width='60%'>
      <xsl:value-of            select="."/>
      </td>
       </tr>
      </table>
      </xsl:for-each>
     
      <xsl:for-each           select="Person/PersonLocations/
                                       OtherPracticeLocation  
                                       [@cdr:id=$OrgFrag]/
                                       SpecificPostalAddress">
      
     <table width='90%'>
      <tr>
      <th align='left' width='40%'>
      <xsl:text>&#xa0;&#xa0;&#xa0;City</xsl:text>
      </th>
      <td width='60%'>
      <xsl:value-of               select="City"/>
       </td>
       </tr>
       </table>
        <table width='90%'>                       
     <tr>
   <th align='left' width='40%'>
   <xsl:text>&#xa0;&#xa0;&#xa0;City Suffix</xsl:text>
   </th>
   <td width='60%'>
  <xsl:value-of        select="CitySuffix"/>
  </td>
  </tr>
  </table>
 
 
  <table width='90%'>                       
    <tr>
   <th align='left' width='40%'>
   <xsl:text>&#xa0;&#xa0;&#xa0;State/Political Unit</xsl:text>
   </th>
   <td width='60%'>
   <xsl:value-of        select="PoliticalSubUnit_State/
                                PoliticalSubUnitFullName"/>
  </td>
  </tr>
  </table>
 
   <table width='90%'> 
   <tr>                      
   <th align='left' width='40%'>
   <xsl:text>&#xa0;&#xa0;&#xa0;Country</xsl:text>
   </th>
   <td width='60%'>
   <xsl:value-of        select="Country/
                                CountryFullName"/>
  </td>
  </tr>
  </table>
   <table width='90%'> 
   <tr>                      
   <th align='left' width='40%'>
   <xsl:text>&#xa0;&#xa0;&#xa0;Zip/Postal Code</xsl:text>
   </th>
   <td width='60%'>
   <xsl:value-of        select="PostalCode_ZIP"/>
        
  </td>
  </tr>
  </table>
    <table width='90%'> 
   <tr>                      
   <th align='left' width='40%'>
   <xsl:text>&#xa0;&#xa0;&#xa0;Phone</xsl:text>
   </th>
   <td width='60%'>
   <xsl:value-of        select="../SpecificPhone"/>
   <xsl:if test="../SpecificPhone/@Public">
   <xsl:text>(Public=</xsl:text>
   <xsl:value-of select="../SpecificPhone/@Public"/>
   <xsl:text>)</xsl:text>
   </xsl:if>
   <xsl:if  test="not(../SpecificPhone)">
  <i>
 <xsl:value-of    select=" ../OrganizationLocation/
                       Organization/OrganizationLocations/
                       OrganizationLocation/Location/Phone"/>
                                                            
  </i>
    <xsl:if test="../OrganizationLocation/Organization/
                  OrganizationLocations/
                  OrganizationLocation/Location/Phone/@Public">
    
                 <xsl:text> (Public=</xsl:text>
    <xsl:value-of   select="../OrganizationLocation/Organization/
                            OrganizationLocations/
                            OrganizationLocation/Location/Phone/@Public"/>
    <xsl:text>)</xsl:text>
   </xsl:if>
    </xsl:if>
    </td>
    </tr>
    </table>
       <table width='90%'> 
   <tr>                      
   <th align='left' width='40%'>
   <xsl:text>&#xa0;&#xa0;&#xa0;Toll Free Phone</xsl:text>
   </th>
   <td width='60%'>
   <xsl:value-of        select="../SpecificTollFreePhone"/>
   <xsl:if  test="not(../SpecificTollFreePhone)">
   <i>
  <xsl:value-of         select="../OrganizationLocation/Organization/
                                OrganizationLocations/
                                OrganizationLocation/Location/TollFreePhone"/>
</i>
   </xsl:if>
   </td>
   </tr>
   </table>
    <table width='90%'> 
   <tr>                      
   <th align='left' width='40%'>
   <xsl:text>&#xa0;&#xa0;&#xa0;Fax</xsl:text>
   </th>
   <td width='60%'>
   <xsl:value-of        select="../SpecificFax"/>
    <xsl:if test="not(../SpecificFax)">
