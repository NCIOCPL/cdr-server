<schema              xmlns           = 'http://www.w3.org/2001/XMLSchema'>

 <annotation>
  <appInfo>
   <pattern>
    <rule            context         = 'InScopeProtocol'>
     <assert         test            = 'count(ProtocolTitle
                                          [@Type = "Professional"]) &lt; 2'
     >Only one professional protocol title is allowed.</assert>
     <assert         test            = 'count(ProtocolTitle
                                          [@Type = "Patient"]) &lt; 2'
     >Only one patient protocol title is allowed.</assert>
     <assert         test            = 'count(ProtocolTitle
                                          [@Type = "Original"]) &lt; 2'
     >Only one original protocol title is allowed.</assert>
    </rule>
   </pattern>
   <pattern>
    <rule            context         = 'ArmOrGroup'>
     <assert         test            = 'not(ArmOrGroupLabel =
                                        preceding-sibling::ArmOrGroup
                                        /ArmOrGroupLabel)'
     >ArmOrGroupLabel must be unique.</assert>
    </rule>
   </pattern>
   <pattern>
    <rule            context         = 'ArmsOrGroups'>
     <assert         test            = 'not(@SingleArmStudy="Yes" and
                                         count(ArmOrGroup) &gt; 0)'
     >Single arm study cannot have any ArmOrGroup elements</assert>
     <assert         test            = '@SingleArmStudy="Yes" or
                                        count(ArmOrGroup) &gt; 1'
     >Must have more than one ArmOrGroup or SingleArmStudy attribute</assert>
    </rule>
   </pattern>
   <!--
   <pattern>
    <rule            context         = 'ArmOrGroupLabel'>
     <assert         test            = 'not(not(. = //ArmOrGroupLink))'
     >ArmOrGroupLabel does not have matching ArmOrGroupLink.</assert>
    </rule>
   </pattern>
   -->
   <pattern>
    <rule            context         = 'ArmOrGroupLink'>
     <assert         test            = 'not(not(. = //ArmOrGroupLabel))'
     >ArmOrGroupLink does not have matching ArmOrGroupLabel.</assert>
    </rule>
   </pattern>
  </appInfo>
 </annotation>

<!--=====================================================================-->
<!--                    COMMON PROTOCOL COMPLEX TYPES                    -->
<!--=====================================================================-->
  <complexType       name            = 'ProtocolIDs'>
    <sequence>
      <element       name            = 'PrimaryID'
                     type            = 'PrimaryID'/>
      <element       name            = 'OtherID'
                     type            = 'OtherID'
                     minOccurs       = '0'
                     maxOccurs       = 'unbounded'/>
    </sequence>
  </complexType>

  <complexType       name            = 'PrimaryID'>
    <sequence>
      <element       name            = 'IDString'
                     type            = 'NotEmptyString'/>
      <element       name            = 'Comment'
                     type            = 'Comment'
                     minOccurs       = '0'/>
    </sequence>
  </complexType>

  <complexType       name            = 'OtherID'>
    <sequence>
      <element       name            = 'IDType'
                     type            = 'IDType'/>
      <element       name            = 'IDString'
                     type            = 'NotEmptyString'/>
      <element       name            = 'Comment'
                     type            = 'Comment'
                     minOccurs       = '0'/>
    </sequence>
    <attribute       name            = 'Institution'
                     type            = 'string'
                     minOccurs       = '0'/>
  </complexType>

  <complexType       name            = 'ProtocolAmendmentInformation'>
    <sequence>
      <element       name            = 'ProtocolAmendment'
                     type            = 'ProtocolAmendment'
                     minOccurs       = '0'
                     maxOccurs       = 'unbounded'/>
      <!--ProtocolAmendment is optional in order to allow the situation when 
          DatedAction for future amendment has been entered but no amendment happend yet-->
      <element       name            = 'DatedAction'
                     type            = 'DatedAction'
                     minOccurs       = '0'
                     maxOccurs       = 'unbounded'/>
    </sequence>
  </complexType>

  <complexType       name            = 'ProtocolAmendment'>
    <sequence>
      <element       name            = 'AmendmentID'
                     type            = 'NotEmptyString'
                     minOccurs       = '0'/>
      <element       name            = 'AmendmentDate'
                     type            = 'date'/>
      <element       name            = 'AmendmentSource'
                     type            = 'AmendmentSource'/>
      <element       name            = 'DateReceived'
                     type            = 'date'/>
      <element       name            = 'AmendmentStatus'
                     type            = 'AmendmentStatus'
                     minOccurs       = '0'/>
      <element       name            = 'DateCompleted'
                     type            = 'date'/>
      <element       name            = 'ChangeType'
                     type            = 'ChangeType'
                     maxOccurs       = 'unbounded'/>
      <element       name            = 'Comment'
                     type            = 'Comment'
                     minOccurs       = '0'/>
    </sequence>
  </complexType>

  <complexType       name            = 'ProtocolTitle'
                     mixed           = 'true'>
    <choice          minOccurs       = '0'
                     maxOccurs       = 'unbounded'>
      <group         ref             = 'TitleMarkupElements'/>
    </choice>
    <attribute       name            = 'Type'
                     type            = 'TitleType'/>
  </complexType>

  <complexType       name            = 'ProtocolAbstract'>
    <sequence>
      <element       name            = 'Professional'
                     type            = 'Professional'/>
      <element       name            = 'Patient'
                     type            = 'Patient'/>
    </sequence>
  </complexType>

  <complexType       name            = 'Professional'>
    <sequence>
