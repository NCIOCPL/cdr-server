                     type            = 'ParaLevel'/>
      <element       name            = 'Outcome'
                     type            = 'Outcome'
                     minOccurs       = '0'
                     maxOccurs       = 'unbounded'/>
      <element       name            = 'Outline'
                     type            = 'ParaLevel'/>
      <element       name            = 'ArmsOrGroups'
                     type            = 'ArmsOrGroups'
                     minOccurs       = '0'/>
      <element       name            = 'EntryCriteria'
                     type            = 'EntryCriteria'/>
      <element       name            = 'ProjectedAccrual'
                     type            = 'ParaLevel'
                     minOccurs       = '0'/>
      <element       name            = 'EndPoints'
                     type            = 'ParaLevel'
                     minOccurs       = '0'/>
      <element       name            = 'Stratification'
                     type            = 'ParaLevel'
                     minOccurs       = '0'/>
      <element       name            = 'SpecialStudyParameters'
