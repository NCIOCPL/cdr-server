<schema              xmlns           = 'http://www.w3.org/2001/XMLSchema'>

  <annotation>
    <documentation>This schema is for Citation</documentation>
  </annotation>

<!--=================================================================-->
<!--                COMMON CDR SCHEMA COMPONENTS                     -->
<!--=================================================================-->
  <include           schemaLocation  = 'CdrCommonSchema.xml'/>

<!--=================================================================-->
<!--                 TOP-LEVEL DOCUMENT ELEMENT                      -->
<!--=================================================================-->
  <element           name            = 'Citation'
                     type            = 'Citation'/>

<!--=================================================================-->
<!--                      DOCUMENT TYPE                              -->
<!--=================================================================-->
  <complexType       name            = 'Citation'>
    <sequence>
      <element       name            = 'VerificationDetails'
                     type            = 'VerificationDetails'/>
      <element       name            = 'Comment'
                     type            = 'string'
                     minOccurs       = '0'/>
      <choice>
        <element     name            = 'PubmedArticle'
                     type            = 'PubmedArticle'/>
        <element     name            = 'PDQCitation'
                     type            = 'PDQCitation'/>
      </choice>
      <element       name            = 'DatedAction'
                     type            = 'DatedAction'
                     minOccurs       = '0'
                     maxOccurs       = 'unbounded'/>
      <element       name            = 'DateLastModified'
                     type            = 'date'
                     minOccurs       = '0'/>
      <element       name            = 'PdqKey'
                     type            = 'string'
                     minOccurs       = '0'/>
    </sequence>
    <attribute       name            = 'xmlns:cdr'
                     type            = 'anyURI'
                     minOccurs       = '0'/>
  </complexType>

<!--=================================================================-->
<!--                    OTHER COMPLEX TYPES                          -->
<!--=================================================================-->
  <complexType       name            = 'VerificationDetails'>
    <sequence>
      <element       name            = 'Verified'
                     type            = 'YesNo'/>
      <element       name            = 'VerifiedIn'
                     type            = 'VerifiedIn'
                     minOccurs       = '0'/>
    </sequence>
  </complexType>

  <complexType       name            = 'PubmedArticle'>
    <sequence>
      <choice>
        <element     name            = 'NCBIArticle'
                     type            = 'NCBIArticle'/>
        <element     name            = 'MedlineCitation'
                     type            = 'MedlineCitation'/>
      </choice>
      <element       name            = 'PubmedData'
                     type            = 'PubmedData'
                     minOccurs       = '0'/>
      <element       name            = 'ModifiedRecord'
                     type            = 'Yes'
                     minOccurs       = '0'/>
      <!-- default should be set to "No" -->
      <element       name            = 'Comment'
                     type            = 'string'
                     minOccurs       = '0'/>
    </sequence>
  </complexType>

  <complexType       name            = 'NCBIArticle'>
    <sequence>
      <element       name            = 'PMID'
                     type            = 'string'/>
      <element       name            = 'Article'
                     type            = 'Article'/>
      <element       name            = 'MedlineJournalInfo'
                     type            = 'MedlineJournalInfo'
                     minOccurs       = '0'/>
    </sequence>
  </complexType>

  <complexType       name            = 'MedlineCitation'>
    <sequence>
      <element       name            = 'MedlineID'
                     type            = 'string'
                     minOccurs       = '0'/>
      <element       name            = 'PMID'
                     type            = 'string'/>
      <element       name            = 'DateCreated'
                     type            = 'CitationDate'/>
      <element       name            = 'DateCompleted'
                     type            = 'CitationDate'
                     minOccurs       = '0'/>
      <element       name            = 'DateRevised'
                     type            = 'CitationDate'
                     minOccurs       = '0'/>
      <element       name            = 'Article'
                     type            = 'Article'/>
      <element       name            = 'MedlineJournalInfo'
                     type            = 'MedlineJournalInfo'/>
      <element       name            = 'ChemicalList'
                     type            = 'ChemicalList'
                     minOccurs       = '0'/>
      <element       name            = 'CitationSubset'
                     type            = 'string'
                     minOccurs       = '0'
                     maxOccurs       = 'unbounded'/>
      <element       name            = 'CommentsCorrections'
                     type            = 'CommentsCorrections'
                     minOccurs       = '0'/>
      <element       name            = 'GeneSymbolList'
                     type            = 'GeneSymbolList'
                     minOccurs       = '0'/>
      <element       name            = 'MeshHeadingList'
                     type            = 'MeshHeadingList'
                     minOccurs       = '0'/>
      <element       name            = 'NumberOfReferences'
                     type            = 'string'
                     minOccurs       = '0'/>
      <element       name            = 'PersonalNameSubjectList'
                     type            = 'PersonalNameSubjectList'
                     minOccurs       = '0'/>
      <element       name            = 'OtherID'
                     type            = 'OtherID'
                     minOccurs       = '0'
                     maxOccurs       = 'unbounded'/>
      <element       name            = 'OtherAbstract'
                     type            = 'OtherAbstract'
                     minOccurs       = '0'
                     maxOccurs       = 'unbounded'/>
      <element       name            = 'KeywordList'
                     type            = 'KeywordList'
                     minOccurs       = '0'
                     maxOccurs       = 'unbounded'/>
      <element       name            = 'SpaceFlightMission'
                     type            = 'string'
                     minOccurs       = '0'
                     maxOccurs       = 'unbounded'/>
      <element       name            = 'InvestigatorList'
                     type            = 'InvestigatorList'
                     minOccurs       = '0'/>
      <element       name            = 'GeneralNote'
                     type            = 'OwnedString'
                     minOccurs       = '0'
                     maxOccurs       = 'unbounded'/>
      <element       name            = 'Comment'
                     type            = 'string'
                     minOccurs       = '0'/>
    </sequence>
    <attribute       name            = 'Owner'
                     type            = 'Owner'
                     minOccurs       = '0'/>
    <attribute       name            = 'Status'
                     type            = 'CitationStatus'
                     minOccurs       = '0'/>
  </complexType>

  <complexType       name            = 'Article'>
    <sequence>
      <choice>
        <element     name            = 'Journal'
                     type            = 'Journal'/>
        <element     name            = 'Book'
                     type            = 'Book'/>
      </choice>
      <element       name            = 'ArticleTitle'
                     type            = 'Title'/>
      <element       name            = 'Pagination'
                     type            = 'Pagination'/>
      <element       name            = 'Abstract'
                     type            = 'Abstract'
                     minOccurs       = '0'/>
      <element       name            = 'Affiliation'
                     type            = 'string'
                     minOccurs       = '0'/>
      <element       name            = 'AuthorList'
                     type            = 'AuthorList'
                     minOccurs       = '0'/>
      <element       name            = 'Language'
                     type            = 'string'
                     minOccurs       = '0'/>
      <element       name            = 'DataBankList'
                     type            = 'DataBankList'
                     minOccurs       = '0'/>
      <element       name            = 'GrantList'
                     type            = 'GrantList'
                     minOccurs       = '0'/>
      <element       name            = 'PublicationTypeList'
                     type            = 'PublicationTypeList'/>
      <element       name            = 'VernacularTitle'
                     type            = 'string'
                     minOccurs       = '0'/>
      <element       name            = 'DateOfElectronicPublication'
                     type            = 'string'
                     minOccurs       = '0'/>
    </sequence>
  </complexType>

  <complexType       name            = 'Book'>
    <sequence>
      <element       name            = 'PubDate'
                     type            = 'PubDate'/>
      <element       name            = 'Publisher'
                     type            = 'string'/>
      <element       name            = 'Title'
                     type            = 'Title'/>
      <element       name            = 'AuthorList'
                     type            = 'AuthorList'
                     minOccurs       = '0'/>
      <element       name            = 'CollectionTitle'
                     type            = 'string'
                     minOccurs       = '0'/>
      <element       name            = 'Volume'
                     type            = 'string'
                     minOccurs       = '0'/>
    </sequence>
  </complexType>

  <complexType       name            = 'Journal'>
    <sequence>
      <element       name            = 'ISSN'
                     type            = 'string'
                     minOccurs       = '0'/>
      <element       name            = 'JournalIssue'
                     type            = 'JournalIssue'/>
      <element       name            = 'Coden'
                     type            = 'string'
                     minOccurs       = '0'/>
      <element       name            = 'Title'
                     type            = 'Title'
                     minOccurs       = '0'/>
      <element       name            = 'ISOAbbreviation'
                     type            = 'string'
                     minOccurs       = '0'/>
    </sequence>
  </complexType>

  <complexType       name            = 'JournalIssue'>
    <sequence>
      <element       name            = 'Volume'
                     type            = 'string'
                     minOccurs       = '0'/>
      <element       name            = 'Issue'
                     type            = 'string'
                     minOccurs       = '0'/>
      <element       name            = 'PubDate'
                     type            = 'PubDate'/>
    </sequence>
  </complexType>

  <group             name            = 'PubDateGroup'>
    <choice>
      <sequence>
        <element     name            = 'Year'
                     type            = 'integer'/>
        <choice      minOccurs       = '0'>
          <sequence>
            <element name            = 'Month'
                     type            = 'string'/>
            <element name            = 'Day'
                     type            = 'string'
                     minOccurs       = '0'/>
          </sequence>
          <element   name            = 'Season'
                     type            = 'string'/>
        </choice>
      </sequence>
      <element       name            = 'MedlineDate'
                     type            = 'string'/>
    </choice>
  </group>

  <complexType       name            = 'PubDate'>
    <group           ref             = 'PubDateGroup'/>
  </complexType>

  <complexType       name            = 'Pagination'>
    <choice>
      <sequence>
        <element     name            = 'StartPage'
                     type            = 'string'/>
        <element     name            = 'EndPage'
                     type            = 'string'
                     minOccurs       = '0'/>
        <element     name            = 'MedlinePgn'
                     type            = 'string'
                     minOccurs       = '0'/>
      </sequence>
      <element       name            = 'MedlinePgn'
                     type            = 'string'/>
    </choice>
  </complexType>

  <complexType       name            = 'Abstract'>
    <sequence>
      <!-- If PDQ Citations need ParaLevel for the abstract text, it will
           need to use another name instead of AbstractText.  PhraseLevel
           would be OK, though.
        -->
      <element       name            = 'AbstractText'
                     type            = 'string'/>
      <element       name            = 'CopyrightInformation'
                     type            = 'PhraseLevel'
                     minOccurs       = '0'/>
    </sequence>
  </complexType>

  <complexType       name            = 'OtherAbstract'>
    <sequence>
      <element       name            = 'AbstractText'
                     type            = 'string'/>
      <element       name            = 'CopyrightInformation'
                     type            = 'PhraseLevel'
                     minOccurs       = '0'/>
    </sequence>
    <attribute       name            = 'Type'
                     type            = 'OtherAbstractType'/>
  </complexType>

  <complexType       name            = 'AuthorList'>
    <sequence>
      <element       name            = 'Author'
                     type            = 'Author'
                     maxOccurs       = 'unbounded'/>
    </sequence>

    <!-- Used for PDQ Citations -->
    <attribute       name            = 'Complete'
                     type            = 'YesNo'
                     minOccurs       = '0'/>
    <attribute       name            = 'Editors'
                     type            = 'YesNo'
                     minOccurs       = '0'/>

    <!-- Used for PubMed Citations -->
    <attribute       name            = 'CompleteYN'
                     type            = 'YN'
                     minOccurs       = '0'/>
  </complexType>

  <complexType       name            = 'Author'>
    <sequence>
      <choice>
        <group       ref             = 'PersonalName'/>
        <element     name            = 'CollectiveName'
                     type            = 'string'/>
      </choice>
      <element       name            = 'Affiliation'
                     type            = 'string'
                     minOccurs       = '0'/>
    </sequence>
  </complexType>

  <group             name            = 'PersonalName'>
    <sequence>
      <element       name            = 'LastName'
                     type            = 'string'/>
      <choice        minOccurs       = '0'>
        <element     name            = 'ForeName'
                     type            = 'string'/>
        <sequence>
          <element   name            = 'FirstName'
                     type            = 'string'/>
          <element   name            = 'MiddleName'
                     type            = 'string'
                     minOccurs       = '0'/>
        </sequence>
      </choice>
      <element       name            = 'Initials'
                     type            = 'string'
                     minOccurs       = '0'/>
      <element       name            = 'Suffix'
                     type            = 'string'
                     minOccurs       = '0'/>
    </sequence>
  </group>

  <complexType       name            = 'MedlineJournalInfo'>
    <sequence>
      <!-- The following works because the attributes in a CountryLink are
           optional.
        -->
      <element       name            = 'Country'
                     type            = 'CountryLink'
                     minOccurs       = '0'/>
      <element       name            = 'MedlineTA'
                     type            = 'string'/>
      <element       name            = 'MedlineCode'
                     type            = 'string'
                     minOccurs       = '0'/>
      <element       name            = 'NlmUniqueID'
                     type            = 'string'
                     minOccurs       = '0'/>
    </sequence>
  </complexType>

  <complexType       name            = 'CitationDate'>
    <group           ref             = 'NormalDate'/>
  </complexType>

  <group             name            = 'NormalDate'>
    <sequence>
      <element       name            = 'Year'
                     type            = 'integer'/>
      <element       name            = 'Month'
                     type            = 'string'/>
      <element       name            = 'Day'
                     type            = 'string'/>
      <sequence      minOccurs       = '0'>
        <element     name            = 'Hour'
                     type            = 'integer'/>
        <sequence    minOccurs       = '0'>
          <element   name            = 'Minute'
                     type            = 'integer'/>
          <element   name            = 'Second'
                     type            = 'integer'
                     minOccurs       = '0'/>
        </sequence>
      </sequence>
      <!-- additional validations required for years, dates of months 
           (including leap years)
        -->
    </sequence>
  </group>

  <complexType       name            = 'PubmedData'>
    <sequence>
      <element       name            = 'History'
                     type            = 'History'
                     minOccurs       = '0'
                     maxOccurs       = 'unbounded'/>
      <element       name            = 'PublicationStatus'
                     type            = 'string'/>
      <element       name            = 'ProviderId'
                     type            = 'string'
                     minOccurs       = '0'/>
      <element       name            = 'ArticleIdList'
                     type            = 'ArticleIdList'/>
      <element       name            = 'URL'
                     type            = 'URL'
                     minOccurs       = '0'
                     maxOccurs       = 'unbounded'/>
    </sequence>
  </complexType>

  <complexType       name            = 'History'>
    <sequence>
      <element       name            = 'PubMedPubDate'
                     type            = 'PubMedPubDate'
                     maxOccurs       = 'unbounded'/>
    </sequence>
  </complexType>

  <complexType       name            = 'PubMedPubDate'>
    <group           ref             = 'NormalDate'/>
    <attribute       name            = 'PubStatus'
                     type            = 'PubStatus'/>
  </complexType>

  <complexType       name            = 'ArticleIdList'>
    <sequence>
      <element       name            = 'ArticleId'
                     type            = 'ArticleId'
                     maxOccurs       = 'unbounded'/>
    </sequence>
  </complexType>

  <complexType       name            = 'URL'>
    <simpleContent>
      <extension     base            = 'string'>
        <attribute   name            = 'lang'
                     type            = 'ISOLanguageCode'
                     minOccurs       = '0'/>
        <attribute   name            = 'type'
                     type            = 'URLType'
                     minOccurs       = '0'/>
      </extension>
    </simpleContent>
  </complexType>

  <complexType       name            = 'PDQCitation'>
    <!--All parallel to PubMed elements are kept as in PubMed part,
        when PubMed part changes, PDQ part might change also.-->
    <sequence>
      <element       name            = 'CitationType'
                     type            = 'CitationType'/>
      <element       name            = 'CitationTitle'
                     type            = 'Title'/>
      <element       name            = 'AuthorList'
                     type            = 'AuthorList'
                     minOccurs       = '0'/>
      <element       name            = 'PublicationDetails'
                     type            = 'PublicationDetails'
                     minOccurs       = '0'/>
      <element       name            = 'CancerLitID'
                     type            = 'string'
                     minOccurs       = '0'/>
      <element       name            = 'CancerLitAbstract'
                     type            = 'Abstract'
                     minOccurs       = '0'/>
    </sequence>
  </complexType>

  <complexType       name            = 'PublicationDetails'>
    <sequence>
      <element       name            = 'PublishedIn'
                     type            = 'CitationLink'
                     minOccurs       = '0'/>
      <element       name            = 'OtherPublicationInformation'
                     type            = 'PhraseLevel'/>
      <element       name            = 'PublicationYear'
                     type            = 'integer'
                     minOccurs       = '0'/>
    </sequence>
  </complexType>

  <complexType       name            = 'DataBankList'>
    <sequence>
      <element       name            = 'DataBank'
                     type            = 'DataBank'
                     maxOccurs       = 'unbounded'/>
    </sequence>
    <attribute       name            = 'CompleteYN'
                     type            = 'YN'
                     minOccurs       = '0'/>
  </complexType>

  <complexType       name            = 'DataBank'>
    <sequence>
      <element       name            = 'DataBankName'
                     type            = 'string'/>
      <element       name            = 'AccessionNumberList'
                     type            = 'AccessionNumberList'
                     minOccurs       = '0'/>
    </sequence>
  </complexType>

  <complexType       name            = 'AccessionNumberList'>
    <sequence>
      <element       name            = 'AccessionNumber'
                     type            = 'string'
                     maxOccurs       = 'unbounded'/>
    </sequence>
  </complexType>

  <complexType       name            = 'GrantList'>
    <sequence>
      <element       name            = 'Grant'
                     type            = 'Grant'
                     maxOccurs       = 'unbounded'/>
    </sequence>
    <attribute       name            = 'CompleteYN'
                     type            = 'YN'
                     minOccurs       = '0'/>
  </complexType>

  <complexType       name            = 'Grant'>
    <sequence>
      <element       name            = 'GrantID'
                     type            = 'string'
                     minOccurs       = '0'/>
      <element       name            = 'Acronym'
                     type            = 'string'
                     minOccurs       = '0'/>
      <element       name            = 'Agency'
                     type            = 'string'/>
    </sequence>
  </complexType>

  <complexType       name            = 'PublicationTypeList'>
    <sequence>
      <element       name            = 'PublicationType'
                     type            = 'string'
                     maxOccurs       = 'unbounded'/>
    </sequence>
  </complexType>

  <complexType       name            = 'ChemicalList'>
    <sequence>
      <element       name            = 'Chemical'
                     type            = 'Chemical'
                     maxOccurs       = 'unbounded'/>
    </sequence>
  </complexType>

  <complexType       name            = 'Chemical'>
    <sequence>
      <choice>
        <element     name            = 'RegistryNumber'
                     type            = 'string'/>
        <element     name            = 'CASRegistryNumber'
                     type            = 'string'/>
      </choice>
      <element       name            = 'NameOfSubstance'
                     type            = 'string'/>
    </sequence>
  </complexType>

  <complexType       name            = 'CommentsCorrections'>
    <sequence>
      <element       name            = 'CommentOn'
                     type            = 'RefTemplate'
                     minOccurs       = '0'
                     maxOccurs       = 'unbounded'/>
      <element       name            = 'CommentIn'
                     type            = 'RefTemplate'
                     minOccurs       = '0'
                     maxOccurs       = 'unbounded'/>
      <element       name            = 'ErratumIn'
                     type            = 'RefTemplate'
                     minOccurs       = '0'
                     maxOccurs       = 'unbounded'/>
      <element       name            = 'ErratumFor'
                     type            = 'RefTemplate'
                     minOccurs       = '0'
                     maxOccurs       = 'unbounded'/>
      <element       name            = 'RepublishedFrom'
                     type            = 'RefTemplate'
                     minOccurs       = '0'
                     maxOccurs       = 'unbounded'/>
      <element       name            = 'RepublishedIn'
                     type            = 'RefTemplate'
                     minOccurs       = '0'
                     maxOccurs       = 'unbounded'/>
      <element       name            = 'RetractionOf'
                     type            = 'RefTemplate'
                     minOccurs       = '0'
                     maxOccurs       = 'unbounded'/>
      <element       name            = 'RetractionIn'
                     type            = 'RefTemplate'
                     minOccurs       = '0'
                     maxOccurs       = 'unbounded'/>
      <element       name            = 'UpdateIn'
                     type            = 'RefTemplate'
                     minOccurs       = '0'
                     maxOccurs       = 'unbounded'/>
      <element       name            = 'UpdateOf'
                     type            = 'RefTemplate'
                     minOccurs       = '0'
                     maxOccurs       = 'unbounded'/>
      <element       name            = 'SummaryForPatientsIn'
                     type            = 'RefTemplate'
                     minOccurs       = '0'
                     maxOccurs       = 'unbounded'/>
      <element       name            = 'OriginalReportIn'
                     type            = 'RefTemplate'
                     minOccurs       = '0'
                     maxOccurs       = 'unbounded'/>
    </sequence>
  </complexType>

  <complexType       name            = 'RefTemplate'>
    <sequence>
      <element       name            = 'RefSource'
                     type            = 'string'/>
      <choice        minOccurs       = '0'>
        <element     name            = 'PMID'
                     type            = 'string'/>
        <element     name            = 'MedlineID'
                     type            = 'string'/>
      </choice>
      <element       name            = 'Note'
                     type            = 'GeneralText'
                     minOccurs       = '0'/>
    </sequence>
  </complexType>

  <complexType       name            = 'GeneSymbolList'>
    <sequence>
      <element       name            = 'GeneSymbol'
                     type            = 'string'
                     maxOccurs       = 'unbounded'/>
    </sequence>
  </complexType>

  <complexType       name            = 'MeshHeadingList'>
    <sequence>
      <element       name            = 'MeshHeading'
                     type            = 'MeshHeading'
                     maxOccurs       = 'unbounded'/>
    </sequence>
  </complexType>

  <complexType       name            = 'MeshHeading'>
    <sequence>
      <choice>
        <element     name            = 'DescriptorName'
                     type            = 'TopicText'/>
        <element     name            = 'Descriptor'
                     type            = 'TopicText'/>
      </choice>
      <choice>
        <element     name            = 'QualifierName'
                     type            = 'TopicText'
                     minOccurs       = '0'
                     maxOccurs       = 'unbounded'/>
        <element     name            = 'Subheading'
                     type            = 'TopicText'
                     minOccurs       = '0'
                     maxOccurs       = 'unbounded'/>
      </choice>
    </sequence>
  </complexType>

  <complexType       name            = 'TopicText'>
    <simpleContent>
      <extension     base            = 'string'>
        <attribute   name            = 'MajorTopicYN'
                     type            = 'YN'
                     minOccurs       = '0'/>
      </extension>
    </simpleContent>
  </complexType>

  <complexType       name            = 'ArticleId'>
    <simpleContent>
      <extension     base            = 'string'>
        <attribute   name            = 'IdType'
                     type            = 'ArticleIdType'
                     minOccurs       = '0'/>
      </extension>
