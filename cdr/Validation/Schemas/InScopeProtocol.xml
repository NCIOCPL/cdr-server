    <enumeration     value           = 'Pre-therapy diagnostic'/>
    <enumeration     value           = 'Post-therapy diagnostic'/>
    <enumeration     value           = 'During therapy diagnostic'/>
    <enumeration     value           = 'Supportive care during treatment'/>
    <enumeration     value           = 'Supportive care after treatment'/>
  </simpleType>

  <simpleType        name            = 'HighAge'
                     base            = 'integer'>
    <maxInclusive    value           = '120'/>
  </simpleType>

  <simpleType        name            = 'ProtocolPhase'
                     base            = 'string'>
    <enumeration     value           = 'Phase I'/>
    <enumeration     value           = 'Phase II'/>
    <enumeration     value           = 'Phase III'/>
    <enumeration     value           = 'Phase IV'/>
    <enumeration     value           = 'No Phase Specified'/>
  </simpleType>

  <simpleType        name            = 'ProtocolDesign'
                     base            = 'string'>
    <enumeration     value           = 'Randomized controlled'/>
    <enumeration     value           = 'Double-blind method'/>
    <enumeration     value           = 'Single-blind method'/>
    <enumeration     value           = 'PLacebo controlled'/>
    <enumeration     value           = 'Non-randomized controlled'/>
    <enumeration     value           = 'Multicenter'/>
    <enumeration     value           = 'Dose escalation'/>
    <enumeration     value           = 'Cohort study'/>
    <enumeration     value           = 'Open label'/>
    <enumeration     value           = 'Expanded access'/>
  </simpleType>

  <simpleType        name            = 'SpecialCategory'
                     base            = 'string'>
    <enumeration     value           = 'Treatment IND'/>
    <enumeration     value           = 'NCI High Priority Clinical trial'/>
    <enumeration     value           = 'CTSU trial'/>
    <enumeration     value           = 'EPP trial'/>
    <enumeration     value           = 'Expanded access trial'/>
  </simpleType>

  <simpleType        name            = 'ProtocolStatusName'
                     base            = 'string'>
    <enumeration     value           = 'Active'/>
    <enumeration     value           = 'Approved-Not Yet Active'/>
    <enumeration     value           = 'Temporarily Closed'/>
    <enumeration     value           = 'Closed'/>
    <enumeration     value           = 'Completed'/>
    <enumeration     value           = 'Withdrawn'/>
  </simpleType>

  <simpleType        name            = 'CoopNonCoop'
                     base            = 'string'>
    <enumeration     value           = 'Cooperative'/>
    <enumeration     value           = 'Non-Cooperative'/>
  </simpleType>

  <simpleType        name            = 'ProtocolPersonRole'
                     base            = 'string'>
    <enumeration     value           = 'Update Person'/>
    <enumeration     value           = 'Recruitment Contact'/>
    <enumeration     value           = 'Protocol Chair'/>
    <enumeration     value           = 'Protocol Co-Chair'/>
  </simpleType>

  <simpleType        name            = 'SpecificPersonRole'
                     base            = 'string'>
    <enumeration     value           = 'Principal Investigator'/>
    <enumeration     value           = 'Research Nurse'/>
    <enumeration     value           = 'Study Coordinator'/>
