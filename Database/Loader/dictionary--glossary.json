{
  "mappings": {
    "dynamic": "strict",
    "properties": {
      "audience": {
        "type": "keyword"
      },
      "definition": {
        "properties": {
          "html": {
            "type": "keyword"
          },
          "text": {
            "type": "keyword"
          }
        }
      },
      "dictionary": {
        "type": "keyword",
        "normalizer": "caseinsensitive_normalizer"
      },
      "first_letter": {
        "type": "keyword",
        "normalizer": "caseinsensitive_normalizer"
      },
      "language": {
        "type": "keyword"
      },
      "media": {
        "properties": {
          "alt": {
            "type": "keyword"
          },
          "caption": {
            "type": "keyword"
          },
          "hosting": {
            "type": "keyword"
          },
          "image_sources": {
            "properties": {
              "size": {
                "type": "keyword"
              },
              "src": {
                "type": "keyword"
              },
              "template": {
                "type": "keyword"
              }
            }
          },
          "ref": {
            "type": "keyword"
          },
          "template": {
            "type": "keyword"
          },
          "title": {
            "type": "keyword"
          },
          "type": {
            "type": "keyword"
          },
          "unique_id": {
            "type": "keyword"
          }
        }
      },
      "pretty_url_name": {
        "type": "keyword"
      },
      "pronunciation": {
        "properties": {
          "audio": {
            "type": "keyword"
          },
          "key": {
            "type": "keyword"
          }
        }
      },
      "related_resources": {
        "properties": {
          "audience": {
            "type": "keyword"
          },
          "pretty_url_name": {
            "type": "keyword"
          },
          "term_id": {
            "type": "long"
          },
          "text": {
            "type": "keyword"
          },
          "type": {
            "type": "keyword"
          },
          "url": {
            "type": "keyword"
          }
        }
      },
      "term_id": {
        "type": "long"
      },
      "term_name": {
        "type": "keyword",
        "fields": {
          "_autocomplete": {
            "type": "text",
            "analyzer": "autocomplete_index",
            "search_analyzer": "autocomplete_search"
          },
          "_contain": {
            "type": "text",
            "analyzer": "ngram_analyzer",
            "search_analyzer": "lowercase_search"
          }
        },
        "normalizer": "caseinsensitive_normalizer"
      },
      "other_languages": {
        "properties": {
          "language": {
            "type": "keyword"
          },
          "term_name": {
            "type": "keyword"
          },
          "pretty_url_name": {
            "type": "keyword"
          }
        }
      }
    }
  },
  "settings": {
    "index": {
      "number_of_shards": "1",
      "max_ngram_diff": 150,
      "analysis": {
        "filter": {
          "ngram_filter": {
            "token_chars": [
              "letter",
              "digit",
              "punctuation",
              "symbol"
            ],
            "min_gram": "1",
            "type": "ngram",
            "max_gram": "150"
          },
          "autocomplete_filter": {
            "token_chars": [
              "letter",
              "digit",
              "punctuation",
              "symbol"
            ],
            "min_gram": "1",
            "type": "edge_ngram",
            "max_gram": "30"
          }
        },
        "normalizer": {
          "caseinsensitive_normalizer": {
            "filter": [
              "lowercase",
              "asciifolding"
            ],
            "type": "custom"
          }
        },
        "analyzer": {
          "autocomplete_index": {
            "filter": [
              "lowercase",
              "autocomplete_filter",
              "asciifolding"
            ],
            "type": "custom",
            "tokenizer": "standard"
          },
          "lowercase_search": {
            "filter": [
              "lowercase",
              "asciifolding"
            ],
            "tokenizer": "keyword"
          },
          "ngram_analyzer": {
            "filter": [
              "lowercase",
              "ngram_filter",
              "asciifolding"
            ],
            "type": "custom",
            "tokenizer": "keyword"
          },
          "autocomplete_search": {
            "type": "custom",
            "tokenizer": "standard",
            "filter": [
              "lowercase",
              "asciifolding"
            ]
          }
        }
      }
    }
  }
}
