<?xml version="1.0" ?>
<!-- Filter title: Remove Document Filter -->

<xsl:transform xmlns:xsl="http://www.w3.org/1999/XSL/Transform"
               version="1.0"
               xmlns:cdr="cips.nci.nih.gov/cdr">
    <xsl:output method="text"/>

<!-- Filter written by Peter Zhang - 05/10/2002 -->
<!-- This is a filter to simply store a line    -->
<!--    to the filtered document. It is used    -->
<!--    by Primary PublishSystem (Cancer.gov)   -->
<!--    to record removed documents.            -->

    <xsl:template match="*">
        <xsl:text>Removed this document</xsl:text>
    </xsl:template>

</xsl:transform>
