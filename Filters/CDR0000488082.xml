<?xml version="1.0"?>
<!-- Filter title: DocTitle for ClinicalTrialSearchString -->
<xsl:transform     xmlns:xsl = "http://www.w3.org/1999/XSL/Transform"
                     version = "1.0"
                   xmlns:cdr = "cips.nci.nih.gov/cdr">
 <xsl:output method="text"/>

 <!--
     $Id$
  -->

 <!-- ==========================================================
      This filter is used to construct display titles for CDR
      ClinicalTrialSearchString documents.
      ==========================================================-->
 <xsl:template         match = "ClinicalTrialSearchString">
  <xsl:value-of       select = "SearchString"/>
 </xsl:template>

</xsl:transform>
