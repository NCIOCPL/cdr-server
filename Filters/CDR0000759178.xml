                             xmlns:cdr = 'cips.nci.nih.gov/cdr'
                               version = '1.0'>

 <xsl:output                    method = 'xml'
                              encoding = 'utf-8'/>

 <xsl:param                       name = 'sample-param'
                                select = '"default-value"'/>

 <!-- Sample template -->
