<CdrDoc Type='Filter' Id='CDR0000000131'>
<CdrDocCtl>
<DocValStatus readonly="yes">U</DocValStatus>
<DocActiveStatus readonly="yes">A</DocActiveStatus>
<DocTitle readonly="yes">Glossary Link Report Filter</DocTitle>
</CdrDocCtl>
<CdrDocXml><![CDATA[<?xml version="1.0"?>
<xsl:transform             xmlns:xsl = "http://www.w3.org/1999/XSL/Transform"
                           xmlns:cdr = "cips.nci.nih.gov/cdr"
                             version = "1.0">

 <xsl:param                     name = "linkTarget"/>

 <xsl:output                  method = "html"/>

 <xsl:template match="/">
  <xsl:variable                 name = "this"
                              select = "document('cdr:/*/CdrCtl')"/>
  <xsl:for-each               select = "descendant::*">
   <xsl:if                      test = "@cdr:ref=$linkTarget or 
                                        @cdr:href=$linkTarget">
    <xsl:variable               name = "parent"
                              select = "parent::node()" />
    <tr>
     <td                      valign = "top">
      <font                     size = "3">
       <xsl:value-of          select = "document('cdrutil:/docid')"/>
      </font>
     </td>
     <td                      valign = "top">
      <font                     size = "3">
      <xsl:value-of           select = "$this/CdrDocCtl/DocTitle"/>
      </font>
     </td>
     <td                      valign = "top">
      <font                     size = "3">
      <xsl:value-of           select = "name($parent)" />
      </font>
     </td>
     <td                      valign = "top">
      <font                     size = "3">
       <xsl:choose>
        <xsl:when               test = "$parent[@cdr:id]">
         <xsl:value-of        select = "$parent/@cdr:id"/>
        </xsl:when>
        <xsl:otherwise>
         <xsl:text>None</xsl:text>
        </xsl:otherwise>
       </xsl:choose>
      </font>
     </td>
    </tr>
   </xsl:if>
  </xsl:for-each>
 </xsl:template>

</xsl:transform>
]]>
</CdrDocXml>
</CdrDoc>