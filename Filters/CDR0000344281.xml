<CdrDoc Type='Filter' Id='CDR0000344281'>
<CdrDocCtl>
<DocValStatus readonly="yes">U</DocValStatus>
<DocActiveStatus readonly="yes">A</DocActiveStatus>
<DocTitle readonly="yes">Module: HP InScopeProtocol Identification</DocTitle>
<ReadyForReview readonly="yes">N</ReadyForReview>
</CdrDocCtl>
<CdrDocXml><![CDATA[<?xml version="1.0"?>
<xsl:transform             xmlns:xsl = "http://www.w3.org/1999/XSL/Transform"
                             version = "1.0"
                           xmlns:cdr = "cips.nci.nih.gov/cdr"
                           exclude-result-prefixes = "cdr">
 
<!-- ==============================================================
               Display the Identification Information
     =============================================================== -->
     


  <!-- ==============================================================
               Display PDQ Key
     =============================================================== -->
  
  
   <xsl:template     match = "PdqKey">
   <b><font size='4'>Identification Information</font></b>
   <br />
  <table width='90%' cellpadding='0' cellspacing='0'>
  <tr>
  <th align='left' width='40%'>
  <u>
  <b><font size='3'>PDQ ID
  </font>
  </b>
  </u>
  </th>
  <td width='60%'>
  <xsl:value-of                select="."/>
  </td>
  </tr>
  </table>
  </xsl:template>

 <!-- ================================================================
         Display the Primary ID and Other IDs and ID Types, Comments
      =============================================================== -->

  <xsl:template   match  = "PrimaryID">
  <xsl:if         test = "not(../../PdqKey)">
  <b><font size='4'>Identification Information</font></b>
   <br />
  </xsl:if>
  <table width = '90%' cellpadding='0' cellspacing='0'>
  <tr>
  <th align='left' width='40%' valign='top'>
  <xsl:choose>
  <xsl:when        test="not(../../PdqKey)">
  <u>
  <b><font size='3'>Primary ID
  </font>
  </b>
  </u>
  </xsl:when>
  <xsl:otherwise>
  <b><font size='3'>Primary ID
  </font>
  </b>
  </xsl:otherwise>
  </xsl:choose>
  </th>
  <td width='60%'>
 <xsl:apply-templates select="IDString"/><br />
 </td>
 </tr>
 </table>
 <b>
 <font size='3'>Comment
 </font>
  </b>
  <br />
  <xsl:choose>
  <xsl:when                test = "Comment">
  <xsl:value-of          select="Comment"/>
  <br />
  <br />
  </xsl:when>
  <xsl:otherwise>
   <br />
  </xsl:otherwise>
  </xsl:choose>
  </xsl:template>

   <xsl:template               match = "OtherID"> 
   <table width='90%' cellpadding='0' cellspacing='0'>
   <tr align='left' width='40%' valign='top'>
   <th>
   <u>
   <b><font size='3'>Other ID
   </font>
   </b>
   </u>
   </th>
   </tr>
   </table>

   <table width='90%' cellpadding='0' cellspacing='0'>
   <tr>
   <th align='left' width='40%' valign='top'>
   <b><font size='3'>ID Type
   </font>
   </b>
   </th>
   <td width='60%'>
   <xsl:value-of                 select="IDType"/>
   </td>
   </tr>
   </table>
   <table width='90%' cellpadding='0' cellspacing='0'>
   <tr> 
   <th align='left' width='40%' valign='top'>
   <b><font size='3'>ID Number
   </font>
   </b>
   </th>
   <td width='60%'>
   <xsl:value-of      select="IDString"/>
   </td>
   </tr>
   </table>
   <b><font size='3'>Comment
   </font>
   </b>
   <br />
  <xsl:if           test ="not(Comment)">
  <br />
  </xsl:if>
  <xsl:if           test = "Comment">     
  <xsl:value-of      select="Comment"/>
  <br />
  <br />
  </xsl:if>
  </xsl:template>
 
 
<!-- ====================================================================
                 Display Protocol Phase
     ==================================================================== -->

    <xsl:template match="ProtocolPhase">
     
    <table width='90%' cellpadding='0' cellspacing='0'>
    <tr>
    <th align='left' width='40%' valign='top'>
    <xsl:if           test="not(preceding-sibling::ProtocolPhase)">  
    <b>
    <font size='4'>Protocol Phase
    </font>
    </b>
    </xsl:if>
    </th>
    <td width='60%'>
    <xsl:apply-templates/>
    </td>
    </tr>
    </table>
    <xsl:if             test="following-sibling::ProtocolDesign">
    <br />
    </xsl:if>
    </xsl:template>

<!-- ====================================================================
                 Display Protocol Design
     ==================================================================== -->

  <xsl:template       match="ProtocolDesign">
   <table width='90%' cellpadding='0' cellspacing='0'>
   <tr>
   <th align='left' width='40%' valign='top'>
   <xsl:if           test="not(preceding-sibling::ProtocolDesign)">
   <b>
   <font size='4'>Protocol Design
   </font>
   </b>
   </xsl:if>
   </th>
   <td width='60%'> 
   <xsl:apply-templates/>
   </td>
   </tr>
   </table>
   </xsl:template>

</xsl:transform>]]>
</CdrDocXml>
</CdrDoc>