<?xml version="1.0"?>
<!-- Filter title: Country Political SubUnit Report -->
  <xsl:transform xmlns:xsl="http://www.w3.org/1999/XSL/Transform"
                 version="1.0"
        xmlns:cdr="cips.nci.nih.gov/cdr">
  
   <xsl:output method="html"/>
    <xsl:template match="/">
  
    <!-- <!DOCTYPE HTML PUBLIC '-//IETF//DTD HTML//EN' /> -->
    <HTML>
    <HEAD>
       <xsl:variable name="cdrmydoc" select="document('cdrx:/*/CdrCtl')"/>

    <TITLE><xsl:value-of select="document('cdrutil:/docid')"/>&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;<xsl:value-of select="$cdrmydoc/CdrDocCtl/DocTitle"/> </TITLE>
     
       <!--
     <LINK REL='stylesheet' 
           HREF='/stylesheets/protocol2.css' 
           TYPE='text/css' /> -->
    </HEAD>
    <BASEFONT FACE='Arial, Helvetica, sans-serif' /> 
     <xsl:apply-templates select="Country"/>
     </HTML>
    </xsl:template>
  <!-- Generate the HTML for the Country/Political subunit QC Report-->
  <xsl:template match="Country">
    <BODY>
   <!--Display Report heading and subheading -->
  
   <b><font size="4"><center><xsl:text>Country/PoliticalSubUnit QC Report</xsl:text></center></font></b>
<b><font size='3'><center><xsl:value-of select="document('cdrutil:/date/%25B %25#d, %25Y %25#I:%25M%25p')"/></center></font></b><br /><br /><br />
<!-- Display headers and data elements -->
<b><font size="4"><xsl:text>Country Full Name</xsl:text></font></b>
 <xsl:text>&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;</xsl:text>   
<xsl:apply-templates select="CountryFullName"/><br /><br />
<b><font size="4"><xsl:text>Country Short Name</xsl:text></font></b>
 <xsl:text>&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;&#xa0;</xsl:text>   
<xsl:apply-templates select="CountryShortName"/><br /><br />
