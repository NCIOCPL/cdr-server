<?xml version="1.0"?>
<!-- Filter title: DocTitle for PublishingSystem -->

<!-- Filter written by Peter Zhang - 04/17/2002 -->
<!-- This is a filter to generate publishing    -->
<!--    control document title                  -->

<xsl:transform xmlns:xsl="http://www.w3.org/1999/XSL/Transform"
               version="1.0"
               xmlns:cdr="cips.nci.nih.gov/cdr">
  <xsl:output method="text"/>

  <xsl:template match="PublishingSystem">
    <xsl:value-of select="SystemName"/>
  </xsl:template>

</xsl:transform>
