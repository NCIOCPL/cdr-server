<?xml version="1.0" encoding="UTF-8"?>
<configuration>
  <system.webServer>
    <handlers>
      <add name="Python"
           path="*.py"
           verb="*"
           modules="CgiModule"
           scriptProcessor="D:\Python3\python.exe %s %s"
           resourceType="Unspecified"
           requireAccess="Script" />
    </handlers>
    <defaultDocument>
      <files>
        <add value="tunnel.py" />
      </files>
    </defaultDocument>
  </system.webServer>
</configuration>
